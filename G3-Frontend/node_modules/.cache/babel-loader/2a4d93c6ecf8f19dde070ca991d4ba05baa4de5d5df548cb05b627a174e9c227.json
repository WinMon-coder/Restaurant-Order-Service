{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport utils from \"@/utils/utils\";\nimport { mapGetters, mapActions, mapState } from \"vuex\";\nexport default {\n  name: \"SideBarRight\",\n  components: {},\n  data: () => ({\n    localDomain: utils.constant.localDomain,\n    loading: false,\n    errorAlert: false,\n    orderItem: \"\",\n    loginUserName: \"\",\n    loginUserRole: \"\",\n    createDemo: {\n      orderTable: {},\n      orderItems: [],\n      orderUser: {},\n      orderTotal: 0\n    }\n  }),\n  async created() {\n    {\n      {\n        this.loginUser;\n      }\n    }\n    // Login User\n    //this.loginUser = this.$store.state.loginUser;\n    // console.log(this.loginUser.userFullName);\n    this.loginUserName = this.loginUser.userFullName;\n    this.loginUserRole = this.loginUser.userRoles;\n    // console.log((this.loginUserRole = this.loginUser.userRoles));\n  },\n\n  computed: {\n    // orderTotalPrice() {\n    //   return this.$store.getters.orderTotalPrice;\n    // },\n    ...mapGetters([\"orderTotalPrice\", \"getTable\", \"getFoodList\", \"loginUser\"]),\n    ...mapState([\"cart\"])\n  },\n  methods: {\n    // increase(food) {\n    //   this.$store.dispatch(\"IncreaseProdcut\", food);\n    // },\n    // decrease(food) {\n    //   this.$store.dispatch(\"DecreaseProduct\", food);\n    // },\n    // removeFoodFromOrder(food) {\n    //   this.$store.dispatch(\"removeFoodFromOrder\", food);\n    // },\n    // cancelOrderItems() {\n    //   this.$store.dispatch(\"cancelOrderItems\");\n    // },\n    ...mapActions([\"increaseFoodItem\", \"decreaseFoodItem\", \"removeFoodFromOrder\", \"cancelOrder\"]),\n    async saveOrder() {\n      if (!(this.orderTotalPrice <= 0) && this.getTable.table != null) {\n        const resp = await utils.http.post(\"/user/order/create\", {\n          orderTable: this.getTable.table,\n          orderItems: this.cart,\n          orderUser: this.loginUser,\n          orderTotal: this.orderTotalPrice\n        });\n        if (resp && resp.status === 200) {\n          this.$router.push({\n            path: \"/home\"\n          });\n          this.createDemo = {\n            orderTable: this.getTable.table,\n            orderItems: this.cart,\n            orderUser: this.loginUser,\n            orderTotal: this.orderTotalPrice\n          };\n          console.log(this.createDemo.orderTable);\n          this.$router.go();\n        }\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAwPA;AACA;AACA;EACAA;EACAC;EAEAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACA;IACA;MACA;QACA;MACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;EACA;;EACAC;IACA;IACA;IACA;IACA;IACA;EACA;EACAC;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA,eACA,oBACA,oBACA,uBACA,cACA;IAEA;MACA;QACA;UACAL;UACAC;UACAC;UACAC;QACA;QAEA;UACA;YAAAG;UAAA;UACA;YACAN;YACAC;YACAC;YACAC;UACA;UAEAI;UACA;QACA;MACA;IACA;EACA;AACA","names":["name","components","data","localDomain","loading","errorAlert","orderItem","loginUserName","loginUserRole","createDemo","orderTable","orderItems","orderUser","orderTotal","computed","methods","path","console"],"sourceRoot":"src/components","sources":["SideBarRight.vue"],"sourcesContent":["<template>\n  <!-- ---------------------------- Bill Payment SideBar  ---------------------------- -->\n  <v-navigation-drawer app color=\"white\" right width=\"290\">\n    <v-div>\n      <v-row>\n        <v-col cols=\"12\" md=\"7\">\n          <v-list subheader two-line class=\"mt-1\">\n            <v-list-item>\n              <v-list-item-avatar rounded>\n                <v-img src=\"img3.png\"></v-img>\n              </v-list-item-avatar>\n              <v-list-item-content>\n                <div v-for=\"(role, index) in this.loginUserRole\" :key=\"index\">\n                  <v-list-item-subtitle>\n                    I am {{ role.roleName }}\n                  </v-list-item-subtitle>\n                </div>\n                <v-list-item-title>{{ loginUserName }}</v-list-item-title>\n              </v-list-item-content>\n            </v-list-item>\n          </v-list></v-col\n        >\n        <v-col cols=\"12\" md=\"5\">\n          <v-container class=\"mt-3\" v-if=\"getTable.table\">\n            <v-chip small color=\"success\" label class=\"white--text\"\n              ><strong>Table - {{ getTable.table.tableName }}</strong></v-chip\n            >\n          </v-container>\n          <v-container class=\"mt-3\" v-else>\n            <v-chip small color=\"primary\" label class=\"white--text\"\n              ><strong>No Table</strong></v-chip\n            >\n          </v-container>\n        </v-col>\n      </v-row>\n    </v-div>\n\n    <strong class=\"ml-3\">Bills</strong>\n    <!-- ---------------------------- Order Items ---------------------------- -->\n    <div v-for=\"(item, index) in cart\" :key=\"index\">\n      <v-list subheader two-line>\n        <v-list-item>\n          <v-list-item-avatar rounded color=\"grey lighten-4\">\n            <v-img :src=\"localDomain + item.food.foodPhoto\"></v-img>\n          </v-list-item-avatar>\n          <v-row>\n            <v-col cols=\"12\" md=\"6\">\n              <v-list-item-content>\n                <v-list-item-title class=\"subtitle-2\">{{\n                  item.food.foodName\n                }}</v-list-item-title>\n                <v-list-item-subtitle\n                  >{{ item.quantity }} X\n                  <strong>{{ item.food.foodPrice }}Ks</strong>\n                  <!-- <v-btn plain color=\"#800015\" small\n                >No Ice\n                <v-icon right>mdi-pencil</v-icon>\n              </v-btn> -->\n                </v-list-item-subtitle>\n              </v-list-item-content>\n            </v-col>\n            <v-col cols=\"12\" md=\"6\">\n              <v-list-item class=\"justify center text-center\">\n                <v-btn\n                  fab\n                  dark\n                  x-small\n                  color=\"primary\"\n                  @click.prevent=\"increaseFoodItem(item.food)\"\n                >\n                  <v-icon dark>mdi-plus</v-icon>\n                </v-btn>\n\n                <v-btn\n                  fab\n                  dark\n                  x-small\n                  color=\"primary\"\n                  @click.prevent=\"decreaseFoodItem(item.food)\"\n                >\n                  <v-icon dark>mdi-minus</v-icon>\n                </v-btn>\n              </v-list-item>\n            </v-col>\n          </v-row>\n          <v-list-item-action class=\"caption\"\n            >{{ item.quantity * item.food.foodPrice }}Ks\n            <v-icon\n              x-small\n              class=\"red--text\"\n              @click.prevent=\"removeFoodFromOrder(item.food)\"\n            >\n              fas fa-times\n            </v-icon>\n          </v-list-item-action>\n        </v-list-item>\n      </v-list>\n    </div>\n    <!-- ---------------------------- End Order Items  ---------------------------- -->\n    <!-- <v-toolbar flat>\n      <strong>Subtotal</strong><v-spacer></v-spacer><strong>3500Ks</strong>\n    </v-toolbar>\n    <v-toolbar flat class=\"mt-n6\">\n      <span>Tax(10%)</span><v-spacer></v-spacer><span>350Ks</span>\n    </v-toolbar> -->\n    <v-divider class=\"mx-4\"></v-divider>\n    <v-toolbar flat>\n      <strong>Total</strong><v-spacer></v-spacer\n      ><strong>{{ orderTotalPrice }}Ks</strong>\n    </v-toolbar>\n    <!-- ---------------------------- End Total  ---------------------------- -->\n\n    <!-- ---------------------------- Payment Method ---------------------------- -->\n    <!-- <strong class=\"ml-5\">Payment Method</strong> -->\n    <v-item-group mandatory class=\"mt-n1\">\n      <v-container>\n        <v-row justify=\"center\">\n          <!-- ---------------------------- Payment For Cash  ---------------------------- -->\n\n          <!-- <v-col cols=\"12\" md=\"4\">\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-money-bill-wave</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >Cach</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col> -->\n          <!-- ---------------------------- Payment For Credit  ---------------------------- -->\n          <!--            \n          <v-col cols=\"12\" md=\"4\">\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-credit-card</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >debit Card</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col> -->\n          <!-- ---------------------------- Payment For E-Wallet  ---------------------------- -->\n          <!--           \n          <v-col cols=\"12\" md=\"4\">\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-qrcode</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >E-Wallet</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col> -->\n        </v-row>\n        <v-row class=\"mt-1\">\n          <v-col>\n            <v-btn\n              color=\"#F6EFEF\"\n              block\n              dark\n              class=\"widthoutupercase\"\n              @click.prevent=\"saveOrder()\"\n              >Order\n            </v-btn>\n          </v-col>\n          <v-col>\n            <v-btn\n              color=\"#F6EFEF\"\n              block\n              dark\n              class=\"widthoutupercase\"\n              @click.prevent=\"cancelOrder()\"\n              >Cancel</v-btn\n            >\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-item-group>\n  </v-navigation-drawer>\n</template>\n\n<script>\nimport utils from \"@/utils/utils\";\nimport { mapGetters, mapActions, mapState } from \"vuex\";\nexport default {\n  name: \"SideBarRight\",\n  components: {},\n\n  data: () => ({\n    localDomain: utils.constant.localDomain,\n    loading: false,\n    errorAlert: false,\n    orderItem: \"\",\n    loginUserName: \"\",\n    loginUserRole: \"\",\n    createDemo: {\n      orderTable: {},\n      orderItems: [],\n      orderUser: {},\n      orderTotal: 0,\n    },\n  }),\n  async created() {\n    {\n      {\n        this.loginUser;\n      }\n    }\n    // Login User\n    //this.loginUser = this.$store.state.loginUser;\n    // console.log(this.loginUser.userFullName);\n    this.loginUserName = this.loginUser.userFullName;\n    this.loginUserRole = this.loginUser.userRoles;\n    // console.log((this.loginUserRole = this.loginUser.userRoles));\n  },\n  computed: {\n    // orderTotalPrice() {\n    //   return this.$store.getters.orderTotalPrice;\n    // },\n    ...mapGetters([\"orderTotalPrice\", \"getTable\", \"getFoodList\", \"loginUser\"]),\n    ...mapState([\"cart\"]),\n  },\n  methods: {\n    // increase(food) {\n    //   this.$store.dispatch(\"IncreaseProdcut\", food);\n    // },\n    // decrease(food) {\n    //   this.$store.dispatch(\"DecreaseProduct\", food);\n    // },\n    // removeFoodFromOrder(food) {\n    //   this.$store.dispatch(\"removeFoodFromOrder\", food);\n    // },\n    // cancelOrderItems() {\n    //   this.$store.dispatch(\"cancelOrderItems\");\n    // },\n    ...mapActions([\n      \"increaseFoodItem\",\n      \"decreaseFoodItem\",\n      \"removeFoodFromOrder\",\n      \"cancelOrder\",\n    ]),\n\n    async saveOrder() {\n      if (!(this.orderTotalPrice <= 0) && this.getTable.table != null) {\n        const resp = await utils.http.post(\"/user/order/create\", {\n          orderTable: this.getTable.table,\n          orderItems: this.cart,\n          orderUser: this.loginUser,\n          orderTotal: this.orderTotalPrice,\n        });\n\n        if (resp && resp.status === 200) {\n          this.$router.push({ path: \"/home\" });\n          this.createDemo = {\n            orderTable: this.getTable.table,\n            orderItems: this.cart,\n            orderUser: this.loginUser,\n            orderTotal: this.orderTotalPrice,\n          };\n\n          console.log(this.createDemo.orderTable);\n          this.$router.go();\n        }\n      }\n    },\n  },\n};\n</script>\n\n<style>\n.v-card.borderme {\n  border: 2px solid #800015 !important;\n}\n.col-12 {\n  padding: 5px !important;\n}\n.v-btn.widthoutupercase {\n  color: #800015 !important;\n  text-transform: none !important;\n}\n.v-btn:hover {\n  background-color: #800015 !important;\n  color: white !important;\n}\n.v-btn--fab.v-size--x-small {\n  height: 15px;\n  width: 15px;\n  margin: 2px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}