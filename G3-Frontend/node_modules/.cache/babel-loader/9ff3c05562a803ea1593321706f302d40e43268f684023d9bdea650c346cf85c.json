{"ast":null,"code":"import SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\nimport BillPayment from \"@/components/BillPayment.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"TableView\",\n  components: {\n    SideBar,\n    SideBarRight,\n    BillPayment\n  },\n  data: () => ({\n    tableList: []\n  }),\n  mounted() {\n    this.$store.dispatch(\"chooseTable\");\n    //this.$store.dispatch(\"getFood\", this.foodId);\n  },\n\n  async created() {\n    await this.fetchAllTables();\n  },\n  methods: {\n    chooseTable(table) {\n      this.$store.dispatch(\"chooseTable\", {\n        table: table\n      });\n    },\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list_with_order\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA2GA;AACA;AAEA;AACA;AACA;EACAA;EACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACAC;EACA;EACAC;IACA;IACA;EACA;;EACA;IACA;EACA;EAEAC;IACAC;MACA;QACAC;MACA;IACA;IACA;IACA;MACA;MACA;QACA;QACA;MACA;QACAC;MACA;IACA;EACA;AACA","names":["name","components","SideBar","SideBarRight","BillPayment","data","tableList","mounted","methods","chooseTable","table","console"],"sourceRoot":"src/views","sources":["TableView.vue"],"sourcesContent":["<template>\n  <v-app :style=\"{ background: $vuetify.theme.themes.dark.background }\">\n    <SideBar />\n    <SideBarRight />\n    <v-dialog v-model=\"dialog\" width=\"500\">\n      <template v-slot:activator=\"{ on, attrs }\">\n        <v-container>\n          <v-item-group mandatory class=\"mt-n1\">\n            <v-container>\n              <v-row justify=\"center\" class=\"space\">\n                <!-- ---------------------------- For Payment Table ---------------------------- -->\n\n                <v-col cols=\"12\" md=\"2\">\n                  <v-item>\n                    <v-card\n                      v-bind=\"attrs\"\n                      v-on=\"on\"\n                      class=\"d-flex align-center rounded-lg mx-2\"\n                      height=\"140\"\n                      flat\n                    >\n                      <v-row>\n                        <v-col cols=\"12\" sm=\"12\">\n                          <v-list-item three-line class=\"text-center\">\n                            <v-list-item-content>\n                              <div align=\"center\" justify=\"center\">\n                                <v-img\n                                  src=\"c1.png\"\n                                  max-height=\"110\"\n                                  max-width=\"110\"\n                                  contain\n                                ></v-img>\n                              </div>\n                              <v-list-item-subtitle class=\"mt-4\"\n                                >Testing Payment</v-list-item-subtitle\n                              >\n                            </v-list-item-content>\n                          </v-list-item>\n                        </v-col>\n                      </v-row>\n                    </v-card>\n                  </v-item>\n                </v-col>\n                <!-- ----------------------------End For Payment Table  ---------------------------- -->\n                <!-- ---------------------------- Table ---------------------------- -->\n\n                <v-col\n                  cols=\"12\"\n                  md=\"2\"\n                  v-for=\"(table, index) in tableList\"\n                  :key=\"index\"\n                >\n                  <v-item>\n                    <v-card\n                      class=\"d-flex align-center rounded-lg mx-2\"\n                      height=\"140\"\n                      flat\n                    >\n                      <v-row>\n                        <v-col cols=\"12\" sm=\"12\">\n                          <v-list-item\n                            three-line\n                            class=\"text-center\"\n                            @click=\"chooseTable(table)\"\n                            to=\"/menu\"\n                          >\n                            <v-list-item-content>\n                              <div align=\"center\" justify=\"center\">\n                                <v-img\n                                  src=\"c2.png\"\n                                  max-height=\"80\"\n                                  max-width=\"80\"\n                                  contain\n                                ></v-img>\n                              </div>\n                              <br />\n                              <v-list-item-subtitle class=\"mt-2\">\n                                <strong>{{ table.tableName }}</strong>\n                                <br />\n                                <strong>Total - {{ table.billTotal }}</strong>\n                              </v-list-item-subtitle>\n                            </v-list-item-content>\n                          </v-list-item>\n                        </v-col>\n                      </v-row>\n                    </v-card>\n                  </v-item>\n                </v-col>\n                <!-- ---------------------------- End Table ---------------------------- -->\n              </v-row>\n            </v-container>\n          </v-item-group>\n        </v-container>\n      </template>\n\n      <v-card>\n        <v-card-title class=\"text-h5 grey lighten-2\"> Payment </v-card-title>\n\n        <v-card-text>\n          <BillPayment />\n        </v-card-text>\n      </v-card>\n    </v-dialog>\n  </v-app>\n</template>\n\n<script>\nimport SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\n\nimport BillPayment from \"@/components/BillPayment.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"TableView\",\n  components: {\n    SideBar,\n    SideBarRight,\n    BillPayment,\n  },\n  data: () => ({\n    tableList: [],\n  }),\n  mounted() {\n    this.$store.dispatch(\"chooseTable\");\n    //this.$store.dispatch(\"getFood\", this.foodId);\n  },\n  async created() {\n    await this.fetchAllTables();\n  },\n\n  methods: {\n    chooseTable(table) {\n      this.$store.dispatch(\"chooseTable\", {\n        table: table,\n      });\n    },\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list_with_order\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    },\n  },\n};\n</script>\n<style>\n.tx-de {\n  text-decoration: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}