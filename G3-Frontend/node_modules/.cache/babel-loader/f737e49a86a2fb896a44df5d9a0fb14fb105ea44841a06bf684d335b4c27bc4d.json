t>\n                              <div align=\"center\" justify=\"center\">\n                                <v-img\n                                  src=\"c2.png\"\n                                  max-height=\"110\"\n                                  max-width=\"110\"\n                                  contain\n                                ></v-img>\n                              </div>\n                              <br />\n                              <v-list-item-subtitle class=\"mt-2\">\n                                <strong>{{ table.tableName }}</strong>\n                              </v-list-item-subtitle>\n                            </v-list-item-content>\n                          </v-list-item>\n                        </v-col>\n                      </v-row>\n                    </v-card>\n                  </v-item>\n                </v-col>\n                <!-- ----------------------------If Order Table ---------------------------- -->\n                <v-col v-else>\n                  <router-link to=\"/menu\" class=\"tx-de\">\n                    <v-item>\n                      <v-card\n                        class=\"d-flex align-center rounded-lg mx-2\"\n                        height=\"140\"\n                        flat\n                      >\n                        <v-row>\n                          <v-col cols=\"12\" sm=\"12\">\n                            <v-list-item\n                              three-line\n                              class=\"text-center\"\n                              @click=\"chooseTable(table)\"\n                            >\n                              <v-list-item-content>\n                                <div align=\"center\" justify=\"center\">\n                                  <v-img\n                                    src=\"c2.png\"\n                                    max-height=\"110\"\n                                    max-width=\"110\"\n                                    contain\n                                  ></v-img>\n                                </div>\n                                <br />\n                                <v-list-item-subtitle class=\"mt-2\">\n                                  <strong>{{ table.tableName }}</strong>\n                                </v-list-item-subtitle>\n                              </v-list-item-content>\n                            </v-list-item>\n                          </v-col>\n                        </v-row>\n                      </v-card>\n                    </v-item>\n                  </router-link>\n                </v-col>\n              </v-card>\n              <!-- ---------------------------- End Table ---------------------------- -->\n            </v-row>\n          </v-container>\n        </v-item-group>\n      </v-container>\n    </template>\n  </v-app>\n</template>\n\n<script>\nimport SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\n\n//import BillPayment from \"@/components/BillPayment.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"TableView\",\n  components: {\n    SideBar,\n    SideBarRight,\n    // BillPayment,\n  },\n  data: () => ({\n    tableList: [],\n  }),\n\n  async created() {\n    await this.fetchAllTables();\n  },\n  methods: {\n    chooseTable(table) {\n      this.$store.dispatch(\"chooseTable\", {\n        table: table,\n      });\n    },\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list_with_order\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    },\n  },\n};\n</script>\n<style>\n.tx-de {\n  text-decoration: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}