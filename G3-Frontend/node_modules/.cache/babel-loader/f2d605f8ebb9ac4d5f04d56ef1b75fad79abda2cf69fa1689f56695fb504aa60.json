{"ast":null,"code":"import utils from \"@/utils/utils\";\nimport AdminSideBar from \"@/components/AdminSideBar.vue\";\nexport default {\n  name: \"TableView\",\n  components: {\n    AdminSideBar\n  },\n  data: () => ({\n    createTableForm: false,\n    dialog: false,\n    update_dialog: false,\n    delete_dialog: false,\n    loading: false,\n    errorAlert: false,\n    deleteDemo: {},\n    updateDemo: {},\n    createDemo: {\n      tableName: \"\"\n    },\n    tableList: [],\n    headers: [{\n      text: \"id\",\n      align: \"start\",\n      sortable: false,\n      value: \"tableId\"\n    }, {\n      text: \"Table\",\n      value: \"tableName\"\n    }, {\n      text: \"Created Time\",\n      value: \"createdAt\"\n    }, {\n      text: \"Modified Time\",\n      value: \"updatedAt\"\n    }, {\n      text: \"Actions\",\n      value: \"actions\",\n      sortable: false\n    }]\n  }),\n  async created() {\n    await this.fetchAllTables();\n  },\n  methods: {\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    },\n    // <!-- -------------****** save table from utils.http ******-------------------------- -->\n    async save() {\n      if (this.$refs.createTableForm.validate()) {\n        const resp = await utils.http.post(\"/admin/table/create\", {\n          tableName: this.createDemo.tableName\n        });\n        if (resp) {\n          await this.fetchAllTables();\n          this.dialog = false;\n          this.createDemo = {};\n        } else {\n          this.errorAlert = true;\n        }\n      }\n    },\n    // <!-- -------------****** update table from utils.http ******-------------------------- -->\n    async updateItemConfirm(id) {\n      const resp = await utils.http.put(\"/admin/table/update/\" + id, {\n        tableName: this.updateDemo.tableName\n      });\n      if (resp) {\n        await this.fetchAllTables();\n        this.update_dialog = false;\n      }\n    },\n    // <!-- -------------****** delete table from utils.http ******-------------------------- -->\n    async deleteItemConfirm(id) {\n      const resp = await utils.http.del(\"/admin/table/delete/\" + id);\n      if (resp.status == 200) {\n        await this.fetchAllTables();\n        this.delete_dialog = false;\n      }\n    },\n    // <!-- -------------******  event handelar from dialog ******-------------------------- -->\n\n    deleteItem(item) {\n      this.delete_dialog = true;\n      this.deleteDemo = item;\n    },\n    editItem(item) {\n      this.update_dialog = true;\n      this.updateDemo = Object.assign({}, item);\n      this.tableName = this.updateDemo.tableName;\n    },\n    closeUpdate() {\n      this.update_dialog = false;\n    },\n    closeDelete() {\n      this.delete_dialog = false;\n    },\n    saveCancel() {\n      this.dialog = false;\n      this.createDemo = {};\n    }\n  }\n};","map":{"version":3,"mappings":"AAwIA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;MACAC;IACA;IACAC;IACAC,UACA;MACAC;MACAC;MACAC;MACAC;IACA,GACA;MAAAH;MAAAG;IAAA,GACA;MAAAH;MAAAG;IAAA,GACA;MAAAH;MAAAG;IAAA,GACA;MAAAH;MAAAG;MAAAD;IAAA;EAEA;EAEA;IACA;EACA;EACAE;IACA;IACA;MACA;MACA;QACA;QACA;MACA;QACAC;MACA;IACA;IACA;IACA;MACA;QACA;UACAR;QACA;QACA;UACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IAEA;IACA;MACA;QACAA;MACA;MACA;QACA;QACA;MACA;IACA;IACA;IACA;MACA;MACA;QACA;QACA;MACA;IACA;IACA;;IAEAS;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;MACA;IACA;EACA;AACA","names":["name","components","AdminSideBar","data","createTableForm","dialog","update_dialog","delete_dialog","loading","errorAlert","deleteDemo","updateDemo","createDemo","tableName","tableList","headers","text","al