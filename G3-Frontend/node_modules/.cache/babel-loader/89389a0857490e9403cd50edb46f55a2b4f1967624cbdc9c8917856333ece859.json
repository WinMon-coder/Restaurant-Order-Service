{"ast":null,"code":"import SideBar from \"@/components/SideBar.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  props: [\"orderId\"],\n  components: {\n    SideBar\n  },\n  data: () => ({\n    localDomain: utils.constant.localDomain\n  }),\n  mounted() {\n    this.$store.dispatch(\"getOrder\", this.orderId);\n  },\n  computed: {\n    order() {\n      return this.$store.state.order;\n    }\n  },\n  methods: {\n    backTo() {\n      this.$router.go(-1);\n    },\n    capitalized(name) {\n      const capitalizedFirst = name[0].toUpperCase();\n      const rest = name.slice(1);\n      return capitalizedFirst + rest;\n    }\n  }\n};","map":{"version":3,"mappings":"AA6IA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACAC;EACA;EAEAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;MACA;MAEA;IACA;EACA;AACA","names":["props","components","SideBar","data","localDomain","mounted","computed","order","methods","backTo","capitalized"],"sourceRoot":"src/views","sources":["OrderDetail.vue"],"sourcesContent":["<template>\n  <div>\n    <SideBar />\n    <v-row>\n      <v-spacer></v-spacer>\n      <v-col cols=\"12\" md=\"5\">\n        <v-card elevation=\"2\" class=\"mt-6\">\n          <v-container>\n            <span class=\"ml-3\">\n              Date:\n              {{ order.updatedAt | formatDate }}\n            </span>\n            <v-row class=\"ml-2 mr-2 mt-1\">\n              <v-col cols=\"12\" md=\"8\">\n                <strong class=\"primary--text\"\n                  >Bills For : Table - {{ order.orderTable.tableName }}\n                  <v-divider class=\"mx-4\" vertical></v-divider>\n                  Order Id :\n                  {{ order.orderId }}\n                </strong>\n              </v-col>\n\n              <v-col cols=\"12\" md=\"4\" class=\"status\">\n                <v-chip\n                  small\n                  v-if=\"order.orderStatus == 'DONE'\"\n                  color=\"success\"\n                  label\n                  ><h3>Status : {{ order.orderStatus }}</h3></v-chip\n                ><v-chip small v-else color=\"error\" label\n                  ><h3>Status : {{ order.orderStatus }}</h3></v-chip\n                >\n              </v-col>\n            </v-row>\n\n            <!-- ---------------------------- Order Items ---------------------------- -->\n            <v-list\n              subheader\n              two-line\n              class=\"mt-1\"\n              v-for=\"item in order.orderItems\"\n              :key=\"item.orderItemId\"\n            >\n              <v-list-item>\n                <v-list-item-avatar rounded color=\"grey lighten-4\">\n                  <v-img :src=\"localDomain + item.food.foodPhoto\"></v-img>\n                </v-list-item-avatar>\n                <v-list-item-content>\n                  <v-list-item-title class=\"subtitle-2\">{{\n                    capitalized(item.food.foodName)\n                  }}</v-list-item-title>\n                  <v-list-item-subtitle>\n                    {{ item.quantity }} X\n                    <strong>{{ item.food.foodPrice }} Ks</strong>\n                  </v-list-item-subtitle>\n                </v-list-item-content>\n                <v-list-item-action class=\"caption\"\n                  >{{ item.quantity * item.food.foodPrice }}Ks\n                </v-list-item-action>\n              </v-list-item>\n            </v-list>\n\n            <!-- ---------------------------- End Order Items  ---------------------------- -->\n            <!-- <v-toolbar flat>\n      <strong>Subtotal</strong><v-spacer></v-spacer><strong>Ks</strong>\n    </v-toolbar>\n    <v-toolbar flat class=\"mt-n6\">\n      <span>Tax(10%)</span><v-spacer></v-spacer><span>350Ks</span>\n    </v-toolbar> -->\n            <v-divider class=\"mx-4\"></v-divider>\n\n            <v-container class=\"mt-2\" v-if=\"order.orderDiscountTotal > 0\">\n              <v-row class=\"ml-1 mr-1\">\n                <span>Sub Total :</span>\n                <v-spacer></v-spacer>\n                <span>\n                  {{ order.orderTotal + order.orderDiscountTotal }}Ks\n                </span>\n              </v-row>\n              <v-row class=\"ml-1 mr-1\">\n                <span class=\"error--text\">Discount :</span>\n                <v-spacer></v-spacer>\n                <span class=\"error--text\">\n                  {{ order.orderDiscountTotal }}Ks\n                </span>\n              </v-row>\n              <v-row class=\"ml-1 mr-1 mt-6\">\n                <strong class=\"black--text\">Total :</strong>\n                <v-spacer></v-spacer>\n                <strong class=\"black--text\">{{ order.orderTotal }}Ks</strong>\n              </v-row>\n            </v-container>\n\n            <v-toolbar v-else flat>\n              <strong>Total</strong><v-spacer></v-spacer\n              ><strong>{{ order.orderTotal }}Ks</strong>\n            </v-toolbar>\n\n            <v-btn @click.prevent=\"backTo()\" class=\"ma-2\" color=\"primary \">\n              <v-icon left> mdi-arrow-left </v-icon>Back\n            </v-btn>\n\n            <!-- ---------------------------- End Total  ---------------------------- -->\n\n            <!-- ---------------------------- Payment Method ---------------------------- -->\n          </v-container>\n\n          <!-- <v-list\n            subheader\n            two-line\n            class=\"mt-1\"\n            v-for=\"item in order.orderItems\"\n            :key=\"item.orderItemId\"\n          >\n            <v-list-item>\n              <v-list-item-avatar rounded color=\"grey lighten-4\">\n                <v-img :src=\"localDomain + item.food.foodPhoto\"></v-img>\n              </v-list-item-avatar>\n              <v-list-item-content>\n                <v-list-item-title class=\"subtitle-2\">{{\n                  item.food.foodName\n                }}</v-list-item-title>\n                <v-list-item-subtitle>\n                  {{ item.quantity }} X\n                  <strong>{{ item.food.foodPrice }} Ks</strong>\n                </v-list-item-subtitle>\n              </v-list-item-content>\n              <v-list-item-action class=\"caption\"\n                >{{ item.quantity * item.food.foodPrice }}Ks\n              </v-list-item-action>\n            </v-list-item>\n           \n          </v-list> -->\n        </v-card>\n      </v-col>\n      <v-spacer></v-spacer>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport SideBar from \"@/components/SideBar.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  props: [\"orderId\"],\n  components: {\n    SideBar,\n  },\n  data: () => ({\n    localDomain: utils.constant.localDomain,\n  }),\n\n  mounted() {\n    this.$store.dispatch(\"getOrder\", this.orderId);\n  },\n  computed: {\n    order() {\n      return this.$store.state.order;\n    },\n  },\n  methods: {\n    backTo() {\n      this.$router.go(-1);\n    },\n    capitalized(name) {\n      const capitalizedFirst = name[0].toUpperCase();\n      const rest = name.slice(1);\n\n      return capitalizedFirst + rest;\n    },\n  },\n};\n</script>\n\n<style>\n.status {\n  text-align: right;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}