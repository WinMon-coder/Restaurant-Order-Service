m-avatar>\n              <v-list-item-content>\n                <div class=\"overline text-right\">Total Ordered</div>\n                <v-list-item-title class=\"headline mb-1 text-right\"\n                  >245</v-list-item-title\n                >\n              </v-list-item-content>\n            </v-list-item>\n          </v-card>\n        </v-flex>\n        <v-flex sm6 xs12 md6 lg3>\n          <v-card class=\"ma-3\">\n            <v-list-item>\n              <v-list-item-avatar tile class=\"mt-n7\">\n                <v-sheet color=\"#FFC107\" width=\"80\" height=\"80\" elevation=\"10\">\n                  <v-icon dark large>folder_shared</v-icon>\n                </v-sheet>\n              </v-list-item-avatar>\n              <v-list-item-content>\n                <div class=\"overline text-right\">ToDay Order</div>\n                <v-list-item-title class=\"headline mb-1 text-right\"\n                  >17300 Ks</v-list-item-title\n                >\n              </v-list-item-content>\n            </v-list-item>\n          </v-card>\n        </v-flex>\n      </v-layout>\n    </v-container>\n    <template>\n      <v-card>\n        <v-card-title>\n          Order Reports\n          <v-spacer></v-spacer>\n          <v-text-field\n            v-model=\"search\"\n            append-icon=\"mdi-magnify\"\n            label=\"Search\"\n            single-line\n            hide-details\n          ></v-text-field>\n        </v-card-title>\n        <v-data-table :headers=\"headers\" :items=\"orderList\" :search=\"search\"\n          ><template v-slot:[`item.orderStatus`]=\"{ item }\">\n            <v-chip small label :color=\"getColor(item.orderStatus)\" dark>\n              {{ item.orderStatus }}\n            </v-chip>\n          </template>\n        </v-data-table>\n      </v-card>\n    </template>\n  </div>\n</template>\n<script>\nimport SideBar from \"@/components/SideBar.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"OrderView\",\n  components: {\n    SideBar,\n  },\n  data: () => ({\n    search: \"\",\n    headers: [\n      { text: \"ID\", value: \"orderId\" },\n      { text: \"TableNo.\", value: \"orderTable.tableName\" },\n      { text: \"Waiter Name\", value: \"orderUser.userFullName\" },\n      { text: \"Status\", value: \"orderStatus\" },\n      { text: \"Total\", value: \"orderTotal\" },\n      { text: \"Date\", value: \"createdAt\" },\n    ],\n    orderList: [],\n  }),\n  async created() {\n    await this.fetchAllOrders();\n  },\n  computed: {\n    totalAmount: function () {\n      var sum = 0;\n      this.orderList.forEach((e) => {\n        e.orderStatus == \"DONE\";\n        sum += e.orderTotal;\n      });\n      return sum;\n    },\n  },\n  methods: {\n    // <!-- -------------****** get all category from utils.http ******-------------------------- -->\n    async fetchAllOrders() {\n      const response = await utils.http.get(\"/user/order/list\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.orderList = data;\n      }\n    },\n    // <!-- -------------****** save Category from utils.http ******-------------------------- -->\n\n    getColor(orderStatus) {\n      if (orderStatus === \"PROCESSING\") return \"red\";\n      else return \"green\";\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}