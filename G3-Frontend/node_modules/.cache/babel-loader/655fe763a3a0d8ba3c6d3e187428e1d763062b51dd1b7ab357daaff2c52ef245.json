{"ast":null,"code":"import SideBar from \"@/components/SideBar.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"OrderView\",\n  components: {\n    SideBar\n  },\n  data: () => ({\n    search: \"\",\n    headers: [{\n      text: \"ID\",\n      value: \"orderId\"\n    }, {\n      text: \"TableNo.\",\n      value: \"orderTable.tableName\"\n    }, {\n      text: \"Waiter Name\",\n      value: \"orderUser.userFullName\"\n    }, {\n      text: \"Status\",\n      value: \"orderStatus\"\n    }, {\n      text: \"Total\",\n      value: \"orderTotal\"\n    }, {\n      text: \"Date\",\n      value: \"createdAt\"\n    }],\n    orderList: []\n  }),\n  async created() {\n    await this.fetchAllOrders();\n  },\n  computed: {\n    totalAmount: function () {\n      var sum = 0;\n      this.orderList.forEach(e => {\n        e.orderStatus == \"DONE\";\n        sum += e.orderTotal;\n      });\n      return sum;\n    }\n  },\n  methods: {\n    // <!-- -------------****** get all category from utils.http ******-------------------------- -->\n    async fetchAllOrders() {\n      const response = await utils.http.get(\"/user/order/list\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.orderList = data;\n      }\n    },\n    // <!-- -------------****** save Category from utils.http ******-------------------------- -->\n\n    getColor(orderStatus) {\n      if (orderStatus === \"PROCESSING\") return \"red\";else return \"green\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAqGA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACAC;IACAC,UACA;MAAAC;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,GACA;MAAAD;MAAAC;IAAA,EACA;IACAC;EACA;EACA;IACA;EACA;EACAC;IACAC;MACA;MACA;QACAC;QACAC;MACA;MACA;IACA;EACA;EACAC;IACA;IACA;MACA;MACA;QACA;QACA;MACA;IACA;IACA;;IAEAC;MACA,oDACA;IACA;EACA;AACA","names":["name","components","SideBar","data","search","headers","text","value","orderList","comp