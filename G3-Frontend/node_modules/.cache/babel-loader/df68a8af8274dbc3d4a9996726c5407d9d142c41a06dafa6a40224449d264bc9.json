{"ast":null,"code":"import SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\nimport api from \"@/util/api\";\nexport default {\n  name: \"HomeView\",\n  data() {\n    return {\n      categoryList: [],\n      foodList: []\n    };\n  },\n  components: {\n    SideBar,\n    SideBarRight\n  },\n  async created() {\n    await this.getAllCategories();\n    await this.getAllFoods();\n  },\n  methods: {\n    async getAllCategories() {\n      const response = await api.get(\"/category/list\");\n      if (response) {\n        const data = await response.json();\n        this.categoryList = data;\n      }\n    },\n    async getAllFoods() {\n      const response = await api.get(\"/food/list\");\n      if (response) {\n        const data = await response.json();\n        this.foodList = data;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA+KA;AACA;AACA;AACA;EACAA;EACAC;IACA;MACAC;MACAC;IACA;EACA;EAEAC;IACAC;IACAC;EACA;EACA;IACA;IACA;EACA;EACAC;IACA;MACA;MACA;QACA;QACA;MACA;IACA;IACA;MACA;MACA;QACA;QACA;MACA;IACA;EACA;AACA","names":["name","data","categoryList","foodList","components","SideBar","SideBarRight","methods"],"sourceRoot":"src/views","sources":["MenuView.vue"],"sourcesContent":["<template>\n  <v-app :style=\"{ background: $vuetify.theme.themes.dark.background }\">\n    <SideBar />\n    <SideBarRight />\n    <v-container>\n      <v-toolbar color=\"rgba(0,0,0,0)\" flat>\n        <!-- ---------------------------- Label For Category ---------------------------- -->\n        <v-toolbar-title class=\"mt-n3\">Choose Category</v-toolbar-title>\n        <v-spacer></v-spacer>\n        <!-- ---------------------------- Category Search Bar ---------------------------- -->\n        <v-text-field\n          label=\"Search category or menu...\"\n          class=\"mt-5\"\n          color=\"primary\"\n          filled\n          append-icon=\"mdi-magnify\"\n          dense\n          solo\n          flat\n          background-color=\"grey lighten-4\"\n        ></v-text-field>\n        <!-- ---------------------------- End Category Search Bar ---------------------------- -->\n      </v-toolbar>\n      <!-- ---------------------------- Category Menu ---------------------------- -->\n      <v-item-group mandatory class=\"mt-n1\">\n        <v-container>\n          <v-row justify=\"center\" class=\"space\">\n            <!-- ---------------------------- For All Category ---------------------------- -->\n\n            <v-item v-slot=\"{ active, toggle }\" class=\"mt-3\">\n              <v-card\n                :color=\"active ? '#F6EFEF' : 'white'\"\n                :class=\"active ? 'borderme' : 'borderout'\"\n                class=\"d-flex align-center rounded-lg mx-2\"\n                dark\n                height=\"100\"\n                width=\"130\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" sm=\"12\">\n                    <v-list-item three-line class=\"text-center\">\n                      <v-list-item-content>\n                        <div align=\"center\" justify=\"center\">\n                          <v-img\n                            src=\"c1.png\"\n                            max-height=\"110\"\n                            max-width=\"110\"\n                            contain\n                          ></v-img>\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'primary--text' : 'black--text'\"\n                          class=\"caption mt-4\"\n                          >ALL</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n\n            <!-- ----------------------------End For All Category ---------------------------- -->\n\n            <!-- ---------------------------- Category Items ---------------------------- -->\n\n            <v-item\n              class=\"mt-3\"\n              v-slot=\"{ active, toggle }\"\n              v-for=\"(category, index) in categoryList\"\n              :key=\"index\"\n            >\n              <v-card\n                :color=\"active ? '#F6EFEF' : 'white'\"\n                :class=\"active ? 'borderme' : 'borderout'\"\n                class=\"d-flex align-center rounded-lg mx-2\"\n                dark\n                height=\"100\"\n                width=\"130\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" sm=\"12\">\n                    <v-list-item three-line class=\"text-center\">\n                      <v-list-item-content>\n                        <div align=\"center\" justify=\"center\">\n                          <v-img\n                            src=\"c1.png\"\n                            max-height=\"110\"\n                            max-width=\"110\"\n                            contain\n                          ></v-img>\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'primary--text' : 'black--text'\"\n                          class=\"caption mt-4\"\n                          >{{ category.categoryName }}</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n\n            <!-- ---------------------------- End Category Items ---------------------------- -->\n          </v-row>\n        </v-container>\n      </v-item-group>\n\n      <!-- ---------------------------- Label For Item List By Category ---------------------------- -->\n      <v-toolbar color=\"#EEEEEE\" flat>\n        <v-toolbar-title>Coffee Menu</v-toolbar-title><v-spacer></v-spacer\n        ><span color=\"grey\">12 Coffees Results</span>\n      </v-toolbar>\n      <!-- ---------------------------- Show Item List By Category ---------------------------- -->\n      <v-item-group mandatory class=\"mt-n1\">\n        <v-container>\n          <v-row justify=\"center\" class=\"space\">\n            <!-- ---------------------------- Item List ---------------------------- -->\n            <v-col\n              cols=\"12\"\n              md=\"2\"\n              v-for=\"(item, index) in foodList\"\n              :key=\"index\"\n            >\n              <v-item v-slot=\"{ active, toggle }\">\n                <v-card\n                  :color=\"active ? '#F6EFEF' : 'white'\"\n                  :class=\"active ? 'borderme' : 'borderout'\"\n                  class=\"d-flex align-center rounded-lg mx-2\"\n                  dark\n                  height=\"140\"\n                  @click=\"toggle\"\n                  flat\n                >\n                  <v-row>\n                    <v-col cols=\"12\" sm=\"12\">\n                      <v-list-item three-line class=\"text-center\">\n                        <v-list-item-content>\n                          <div align=\"center\" justify=\"center\">\n                            <v-img\n                              src=\"c1.png\"\n                              max-height=\"110\"\n                              max-width=\"110\"\n                              contain\n                            ></v-img>\n                          </div>\n                          <v-list-item-subtitle\n                            :class=\"active ? 'primary--text' : 'black--text'\"\n                            class=\"caption mt-4\"\n                            >{{ item.foodName }} -\n                            <strong\n                              >{{ item.foodPrice }} Ks</strong\n                            ></v-list-item-subtitle\n                          >\n                        </v-list-item-content>\n                      </v-list-item>\n                    </v-col>\n                  </v-row>\n                </v-card>\n              </v-item>\n            </v-col>\n            <!-- ----------------------------End Item List ---------------------------- -->\n          </v-row>\n        </v-container>\n      </v-item-group>\n    </v-container>\n  </v-app>\n</template>\n\n<script>\nimport SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\nimport api from \"@/util/api\";\nexport default {\n  name: \"HomeView\",\n  data() {\n    return {\n      categoryList: [],\n      foodList: [],\n    };\n  },\n\n  components: {\n    SideBar,\n    SideBarRight,\n  },\n  async created() {\n    await this.getAllCategories();\n    await this.getAllFoods();\n  },\n  methods: {\n    async getAllCategories() {\n      const response = await api.get(\"/category/list\");\n      if (response) {\n        const data = await response.json();\n        this.categoryList = data;\n      }\n    },\n    async getAllFoods() {\n      const response = await api.get(\"/food/list\");\n      if (response) {\n        const data = await response.json();\n        this.foodList = data;\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}