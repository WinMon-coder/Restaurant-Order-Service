h\"\n            append-icon=\"mdi-magnify\"\n            label=\"Search\"\n            single-line\n            hide-details\n          ></v-text-field>\n        </v-card-title>\n        <v-data-table :headers=\"headers\" :items=\"foods\" :search=\"search\">\n          <template v-slot:[`item.status`]=\"{ item }\">\n            <v-chip :color=\"getColor(item.status)\" dark>\n              {{ item.foodName }}\n            </v-chip>\n          </template>\n        </v-data-table>\n      </v-card>\n    </template>\n  </v-app>\n</template>\n<script>\nimport AdminSideBar from \"@/components/AdminSideBar.vue\";\nexport default {\n  name: \"AdminView\",\n  components: {\n    AdminSideBar,\n  },\n  props: [\"id\"],\n  data() {\n    return {\n      search: \"\",\n      headers: [\n        { text: \"ID\", value: \"foodId\", sortable: true },\n        { text: \"Name\", value: \"foodName\", sortable: true },\n        {\n          text: \"Category\",\n          value: \"foodCategory.categoryName\",\n          sortable: true,\n        },\n        { text: \"Price\", value: \"foodPrice\", sortable: true },\n      ],\n    };\n  },\n  async created() {},\n  computed: {\n    foods() {\n      return this.$store.state.foods;\n    },\n  },\n  mounted() {\n    this.$store.dispatch(\"getAllFoods\"); //    ...Map\n  },\n\n  methods: {\n    getColor(status) {\n      if (status === \"processing\") return \"red\";\n      else return \"green\";\n    },\n  },\n};\n</script>\n\n<style>\n.v-btn:hover {\n  background-color: #800015 !important;\n  color: #ffffff !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}