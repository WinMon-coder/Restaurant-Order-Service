{"ast":null,"code":"import utils from \"@/utils/utils\";\nimport { mapState } from \"vuex\";\nexport default {\n  name: \"SideBarRight\",\n  components: {},\n  data: () => ({\n    localDomain: utils.constant.localDomain,\n    loading: false,\n    errorAlert: false,\n    loginUserName: \"\",\n    loginUserRole: \"\",\n    orderUser: \"\",\n    orderTable: \"\",\n    foodList: [],\n    orderTotal: 0\n  }),\n  created() {\n    // Login User\n    this.loginUser = this.$store.state.loginUser;\n    // console.log(this.loginUser.userFullName);\n    this.loginUserName = this.loginUser.userFullName;\n    // this.loginUserRole=this.loginUser.userPhoto;\n    console.log(this.loginUserRole = this.loginUser.userRoles);\n  },\n  computed: {\n    orderTotalPrice() {\n      return this.$store.getters.orderTotalPrice;\n    },\n    ...mapState({\n      table: state => state.table,\n      cart: state => state.cart\n    })\n  },\n  methods: {\n    increase(food) {\n      this.$store.dispatch(\"IncreaseProdcut\", food);\n    },\n    decrease(food) {\n      this.$store.dispatch(\"DecreaseProduct\", food);\n    },\n    async saveOrder() {\n      const resp = await utils.http.post(\"/user/order/create\", {\n        orderUser: this.loginUser,\n        orderTable: this.table,\n        foodList: this.cart,\n        orderTotal: this.orderTotalPrice\n        //   foodList: this.$store.getters.cart,\n        //   orderTable: this.$store.getters.table,\n        //   orderTotal: this.orderTotalPrice,\n        // });\n        // console.log(this.orderDetail.orderUser);\n        // console.log(this.orderDetail.orderTable);\n        // console.log(this.orderDetail.foodList);\n        // console.log(this.orderDetail.orderTotal);\n        // if (resp) {\n        //   console.log(resp);\n      });\n    },\n\n    removeFoodFromOrder(food) {\n      this.$store.dispatch(\"removeFoodFromOrder\", food);\n    },\n    cancelOrderItems() {\n      this.$store.dispatch(\"cancelOrderItems\");\n    }\n  }\n};","map":{"version":3,"mappings":"AAmOA;AACA;AACA;EACAA;EACAC;EAEAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;IACA;IACA;IACA;IACA;IACAC;EACA;EACAC;IACAC;MACA;IACA;IACA;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACA;MACA;QACAZ;QACAC;QACAC;QACAC;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;MACA;IACA;;IACAU;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","components","data","localDomain","loading","errorAlert","loginUserName","loginUserRole","orderUser","orderTable","foodList","orderTotal","created","console","computed","orderTotalPrice","table","cart","methods","increase","decrease","removeFoodFromOrder","cancelOrderItems"],"sourceRoot":"src/components","sources":["SideBarRight.vue"],"sourcesContent":["<template>\n  <!-- ---------------------------- Bill Payment SideBar  ---------------------------- -->\n  <v-navigation-drawer app color=\"white\" right width=\"350\">\n    <v-list subheader two-line class=\"mt-1\">\n      <v-list-item>\n        <v-list-item-avatar rounded>\n          <v-img src=\"https://cdn.vuetifyjs.com/images/lists/2.jpg\"></v-img>\n        </v-list-item-avatar>\n        <v-list-item-content>\n          <div v-for=\"(role, index) in this.loginUserRole\" :key=\"index\">\n            <v-list-item-subtitle>\n              I am {{ role.roleName }}</v-list-item-subtitle\n            >\n          </div>\n          <v-list-item-title>{{ loginUserName }}</v-list-item-title>\n        </v-list-item-content>\n      </v-list-item>\n    </v-list>\n    <strong class=\"ml-3\">Bills</strong>\n    <!-- ---------------------------- Order Items ---------------------------- -->\n    <div v-for=\"(item, index) in cart\" :key=\"index\">\n      <v-list subheader two-line>\n        <v-list-item>\n          <v-list-item-avatar rounded color=\"grey lighten-4\">\n            <v-img :src=\"localDomain + item.food.foodPhoto\"></v-img>\n          </v-list-item-avatar>\n          <v-row>\n            <v-col cols=\"12\" md=\"8\">\n              <v-list-item-content>\n                <v-list-item-title class=\"subtitle-2\">{{\n                  item.food.foodName\n                }}</v-list-item-title>\n                <v-list-item-subtitle\n                  >{{ item.quantity }} X\n                  <strong>{{ item.food.foodPrice }}Ks</strong>\n                  <!-- <v-btn plain color=\"#800015\" small\n                >No Ice\n                <v-icon right>mdi-pencil</v-icon>\n              </v-btn> -->\n                </v-list-item-subtitle>\n              </v-list-item-content>\n            </v-col>\n            <v-col cols=\"12\" md=\"4\">\n              <v-list-item>\n                <v-btn\n                  fab\n                  dark\n                  x-small\n                  color=\"primary\"\n                  @click=\"increase(item.food)\"\n                >\n                  <v-icon dark>mdi-plus</v-icon>\n                </v-btn>\n                <v-spacer></v-spacer>\n                <v-btn\n                  fab\n                  dark\n                  x-small\n                  color=\"primary\"\n                  @click=\"decrease(item.food)\"\n                >\n                  <v-icon dark>mdi-minus</v-icon>\n                </v-btn>\n              </v-list-item>\n            </v-col>\n          </v-row>\n          <v-list-item-action class=\"caption\"\n            >{{ item.quantity * item.food.foodPrice }}Ks\n            <v-icon\n              x-small\n              class=\"red--text\"\n              @click.prevent=\"removeFoodFromOrder(item.food)\"\n            >\n              fas fa-times\n            </v-icon>\n          </v-list-item-action>\n        </v-list-item>\n      </v-list>\n    </div>\n    <!-- ---------------------------- End Order Items  ---------------------------- -->\n    <!-- <v-toolbar flat>\n      <strong>Subtotal</strong><v-spacer></v-spacer><strong>3500Ks</strong>\n    </v-toolbar>\n    <v-toolbar flat class=\"mt-n6\">\n      <span>Tax(10%)</span><v-spacer></v-spacer><span>350Ks</span>\n    </v-toolbar> -->\n    <v-divider class=\"mx-4\"></v-divider>\n    <v-toolbar flat>\n      <strong>Total</strong><v-spacer></v-spacer\n      ><strong>{{ orderTotalPrice }}Ks</strong>\n    </v-toolbar>\n    <!-- ---------------------------- End Total  ---------------------------- -->\n\n    <!-- ---------------------------- Payment Method ---------------------------- -->\n    <strong class=\"ml-5\">Payment Method</strong>\n    <v-item-group mandatory class=\"mt-n1\">\n      <v-container>\n        <v-row justify=\"center\">\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For Cash  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-money-bill-wave</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >Cach</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For Credit  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-credit-card</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >debit Card</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For E-Wallet  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-qrcode</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >E-Wallet</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n        </v-row>\n        <v-row class=\"mt-1\">\n          <v-col>\n            <v-btn\n              color=\"#F6EFEF\"\n              block\n              dark\n              class=\"widthoutupercase\"\n              @click.prevent=\"saveOrder()\"\n              >Order\n            </v-btn>\n          </v-col>\n          <v-col>\n            <v-btn\n              color=\"#F6EFEF\"\n              block\n              dark\n              class=\"widthoutupercase\"\n              @click.prevent=\"cancelOrderItems()\"\n              >Cancel</v-btn\n            >\n          </v-col>\n        </v-row>\n        <v-text>{{ table }}</v-text>\n      </v-container>\n    </v-item-group>\n  </v-navigation-drawer>\n</template>\n\n<script>\nimport utils from \"@/utils/utils\";\nimport { mapState } from \"vuex\";\nexport default {\n  name: \"SideBarRight\",\n  components: {},\n\n  data: () => ({\n    localDomain: utils.constant.localDomain,\n    loading: false,\n    errorAlert: false,\n    loginUserName: \"\",\n    loginUserRole: \"\",\n    orderUser: \"\",\n    orderTable: \"\",\n    foodList: [],\n    orderTotal: 0,\n  }),\n  created() {\n    // Login User\n    this.loginUser = this.$store.state.loginUser;\n    // console.log(this.loginUser.userFullName);\n    this.loginUserName = this.loginUser.userFullName;\n    // this.loginUserRole=this.loginUser.userPhoto;\n    console.log((this.loginUserRole = this.loginUser.userRoles));\n  },\n  computed: {\n    orderTotalPrice() {\n      return this.$store.getters.orderTotalPrice;\n    },\n    ...mapState({\n      table: (state) => state.table,\n      cart: (state) => state.cart,\n    }),\n  },\n  methods: {\n    increase(food) {\n      this.$store.dispatch(\"IncreaseProdcut\", food);\n    },\n    decrease(food) {\n      this.$store.dispatch(\"DecreaseProduct\", food);\n    },\n    async saveOrder() {\n      const resp = await utils.http.post(\"/user/order/create\", {\n        orderUser: this.loginUser,\n        orderTable: this.table,\n        foodList: this.cart,\n        orderTotal: this.orderTotalPrice,\n        //   foodList: this.$store.getters.cart,\n        //   orderTable: this.$store.getters.table,\n        //   orderTotal: this.orderTotalPrice,\n        // });\n        // console.log(this.orderDetail.orderUser);\n        // console.log(this.orderDetail.orderTable);\n        // console.log(this.orderDetail.foodList);\n        // console.log(this.orderDetail.orderTotal);\n        // if (resp) {\n        //   console.log(resp);\n      });\n    },\n    removeFoodFromOrder(food) {\n      this.$store.dispatch(\"removeFoodFromOrder\", food);\n    },\n    cancelOrderItems() {\n      this.$store.dispatch(\"cancelOrderItems\");\n    },\n  },\n};\n</script>\n\n<style>\n.v-card.borderme {\n  border: 2px solid #800015 !important;\n}\n.col-12 {\n  padding: 5px !important;\n}\n.v-btn.widthoutupercase {\n  color: #800015 !important;\n  text-transform: none !important;\n}\n.v-btn:hover {\n  background-color: #800015 !important;\n  color: white !important;\n}\n.v-btn--fab.v-size--x-small {\n  height: 20px;\n  width: 20px;\n  margin: 2px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}