{"ast":null,"code":"import SideBar from \"@/components/SideBar.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"OrderView\",\n  components: {\n    SideBar\n  },\n  data: () => ({\n    orderList: [],\n    mostOrdered: [{\n      id: 1,\n      name: \"Burger King Halloween Whopper \",\n      image: \"food-1.png\",\n      count: 150\n    }, {\n      id: 2,\n      name: \"King Whopper Sandwich Hamburger\",\n      image: \"food-2.png\",\n      count: 170\n    }, {\n      id: 3,\n      name: \"Red Robin Gourmet Burger\",\n      image: \"food-3.png\",\n      count: 180\n    }]\n  }),\n  async created() {\n    await this.fetchAllOrders();\n  },\n  methods: {\n    // <!-- -------------****** get all category from utils.http ******-------------------------- -->\n    async fetchAllOrders() {\n      const response = await utils.http.get(\"/user/order/list\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.orderList = data;\n      }\n    },\n    // <!-- -------------****** save Category from utils.http ******-------------------------- -->\n\n    getColor(status) {\n      if (status === \"processing\") return \"red\";else return \"green\";\n    }\n  }\n};","map":{"version":3,"mappings":"AAgFA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EACAC;IACAC;IACAC,cACA;MACAC;MACAN;MACAO;MACAC;IACA,GACA;MACAF;MACAN;MACAO;MACAC;IACA,GACA;MACAF;MACAN;MACAO;MACAC;IACA;EAEA;EACA;IACA;EACA;EACAC;IACA;IACA;MACA;MACA;QACA;QACA;MACA;IACA;IACA;;IAEAC;MACA,+CACA;IACA;EACA;AACA","names":["name","components","SideBar","data","orderList","mostOrdered","id","image","count","methods","getColor"],"sourceRoot":"src/views","sources":["OrderView.vue"],"sourcesContent":["<template>\n  <div>\n    <SideBar />\n    <template>\n      <div class=\"flex flex-col p-6 bg-red-900 rounded-lg gap-y-6\">\n        <div class=\"flex justify-between items-center\">\n          <h2 class=\"text-xl font-semibold leading-loose text-white\">\n            Most Ordered\n          </h2>\n          <button\n            class=\"flex gap-x-2.5 py-3 px-4 rounded-lg border border-gray-200\"\n          >\n            <ChevronDownIcon />\n            <span class=\"text-sm text-white\">Today</span>\n          </button>\n        </div>\n        <hr class=\"border-gray-200\" />\n        <div class=\"flex flex-col gap-y-3\">\n          <div\n            v-for=\"order in mostOrdered\"\n            :key=\"order.id\"\n            class=\"flex gap-x-4 items-center\"\n          >\n            <img class=\"w-14 h-14\" :src=\"`/img/${order.image}`\" alt=\"\" />\n            <div class=\"flex flex-col gap-y-0.5\">\n              <div class=\"text-sm font-medium text-white\">{{ order.name }}</div>\n              <div class=\"text-xs text-gray-200\">{{ order.count }}</div>\n            </div>\n          </div>\n        </div>\n        <button\n          class=\"py-3.5 rounded-lg border border-white text-white text-sm font-semibold\"\n        >\n          View all\n        </button>\n        <div class=\"flex justify-between items-center\">\n          <h2 class=\"text-xl font-semibold text-white leading-loose\">\n            Statistics\n          </h2>\n          <button\n            class=\"flex gap-x-2.5 py-3 px-4 rounded-lg border border-gray-200\"\n          >\n            <ChevronDownIcon />\n            <span class=\"text-sm text-white\">Today</span>\n          </button>\n        </div>\n        <hr class=\"border-gray-200\" />\n        <div class=\"flex gap-x-7\">\n          <img width=\"140\" :src=\"`/img/chart.svg`\" alt=\"\" />\n          <div class=\"flex flex-col gap-y-4\">\n            <div class=\"flex flex-col gap-y-4\">\n              <div class=\"flex gap-x-2 items-start\">\n                <div class=\"w-4 h-4 mt-0.5 rounded-full bg-accent-green\"></div>\n                <div>\n                  <div class=\"text-sm font-semibold text-white\">Dine in</div>\n                  <div class=\"text-xs text-gray-200\">300 customers</div>\n                </div>\n              </div>\n              <div class=\"flex gap-x-2 items-start\">\n                <div class=\"w-4 h-4 mt-0.5 rounded-full bg-accent-orange\"></div>\n                <div>\n                  <div class=\"text-sm font-semibold text-white\">To go</div>\n                  <div class=\"text-xs text-gray-200\">130 customers</div>\n                </div>\n              </div>\n              <div class=\"flex gap-x-2 items-start\">\n                <div class=\"w-4 h-4 mt-0.5 rounded-full bg-accent-blue\"></div>\n                <div>\n                  <div class=\"text-sm font-semibold text-white\">Delivery</div>\n                  <div class=\"text-xs text-gray-200\">70 customers</div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </template>\n  </div>\n</template>\n<script>\nimport SideBar from \"@/components/SideBar.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"OrderView\",\n  components: {\n    SideBar,\n  },\n  data: () => ({\n    orderList: [],\n    mostOrdered: [\n      {\n        id: 1,\n        name: \"Burger King Halloween Whopper \",\n        image: \"food-1.png\",\n        count: 150,\n      },\n      {\n        id: 2,\n        name: \"King Whopper Sandwich Hamburger\",\n        image: \"food-2.png\",\n        count: 170,\n      },\n      {\n        id: 3,\n        name: \"Red Robin Gourmet Burger\",\n        image: \"food-3.png\",\n        count: 180,\n      },\n    ],\n  }),\n  async created() {\n    await this.fetchAllOrders();\n  },\n  methods: {\n    // <!-- -------------****** get all category from utils.http ******-------------------------- -->\n    async fetchAllOrders() {\n      const response = await utils.http.get(\"/user/order/list\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.orderList = data;\n      }\n    },\n    // <!-- -------------****** save Category from utils.http ******-------------------------- -->\n\n    getColor(status) {\n      if (status === \"processing\") return \"red\";\n      else return \"green\";\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}