it utils.http.post(\"/user/order/create\", {\n          orderId: this.getTable.table.order.orderId,\n          orderTable: this.getTable.table,\n          orderItems: this.cart,\n          orderUser: this.loginUser,\n          orderTotal: this.orderTotalPrice\n        });\n        if (resp && resp.status === 200) {\n          let orderObject = {\n            orderId: this.getTable.table.order.orderId,\n            orderTable: this.getTable.table,\n            orderItems: this.cart,\n            orderUser: this.loginUser,\n            orderTotal: this.orderTotalPrice\n          };\n          console.log(\"Demo\" + orderObject.orderTotal);\n          this.createDemo = Object.assign({}, orderObject);\n          console.log(\"Demo\" + this.createDemo.orderTotal);\n          this.$router.push({\n            path: \"/table\"\n          });\n          this.$router.go(0);\n        }\n      }\n    },\n    async fetchUserProfile() {\n      const response = await utils.http.get(\"/user/profile?id=\" + this.loginUser.userId);\n      if (response) {\n        const data = await response.json();\n        if (data) this.loginUser = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AA0UA;AACA;AACA;EACAA;EACAC;EAEAC;IACAC;IACAC;IACAC;IACAC;IACA;IACA;;IAEAC;MACAC;MACAC;MACAC;MACAC;IACA;IACAC;EACA;EACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;EACA;;EACAC;IACA;IACA;IACA;IACA;IACA;EACA;EACAC;IACAC;MACA;MACA;MAEA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA,eACA,oBACA,oBACA,uBACA,cACA;IAEA;MACA;QACA;UACAC;UACAR;UACAC;UACAC;UACAC;QACA;QAEA;UACA;YACAK;YACAR;YACAC;YACAC;YACAC;UACA;UACAM;UACA;UACAA;UACA;YAAAC;UAAA;UACA;QACA;MACA;IACA;IACA;MACA,sCACA,4CACA;MACA;QACA;QACA;MACA;QACAD;MACA;IACA;EACA;AACA","names":["name","components","data","localDomain","loading","errorAlert","orderItem","createDemo","orderTable","orderItems","orderUser","orderTotal","loginUser","computed","methods","capitalized","orderId"