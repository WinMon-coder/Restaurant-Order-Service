{"ast":null,"code":"import SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\nimport BillPayment from \"@/components/BillPayment.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"TableView\",\n  components: {\n    SideBar,\n    SideBarRight,\n    BillPayment\n  },\n  data() {\n    return {\n      payment_dialog: false,\n      tableList: [],\n      table: {}\n    };\n  },\n  async created() {\n    await this.fetchAllTables();\n  },\n  methods: {\n    chooseTable(table) {\n      this.$store.dispatch(\"chooseTable\", {\n        table: table\n      });\n    },\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list_with_order\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAuHA;AACA;AAEA;AACA;AACA;EACAA;EACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACA;IACA;EACA;EACAC;IACAC;MACA;QACAF;MACA;IACA;IACA;IACA;MACA;MACA;QACA;QACA;MACA;QACAG;MACA;IACA;EACA;AACA","names":["name","components","SideBar","SideBarRight","BillPayment","data","payment_dialog","tableList","table","methods","chooseTable","console"],"sourceRoot":"src/views","sources":["TableView.vue"],"sourcesContent":["<template>\n  <!-- :to=\"{ name: 'menu' }\" -->\n  <v-app :style=\"{ background: $vuetify.theme.themes.dark.background }\">\n    <SideBar />\n    <SideBarRight />\n    <template>\n      <div class=\"text-center\">\n        <v-dialog v-model=\"payment_dialog\" width=\"500\">\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-container>\n              <v-item-group mandatory class=\"mt-n1\">\n                <v-container>\n                  <v-row justify=\"center\" class=\"space\">\n                    <v-card\n                      class=\"mr-2 ml-2 mt-2 mb-2\"\n                      v-for=\"(table, index) in tableList\"\n                      :key=\"index\"\n                    >\n                      <!-- ----------------------------If Payment Table ---------------------------- -->\n\n                      <v-col\n                        v-if=\"table.billTotal > 0\"\n                        v-bind=\"attrs\"\n                        v-on=\"on\"\n                      >\n                        <v-item>\n                          <v-card\n                            class=\"d-flex align-center rounded-lg mx-2\"\n                            height=\"140\"\n                            flat\n                          >\n                            <v-row>\n                              <v-col cols=\"12\" sm=\"12\">\n                                <v-list-item\n                                  three-line\n                                  class=\"text-center\"\n                                  @click=\"chooseTable(table)\"\n                                >\n                                  <v-list-item-content>\n                                    <div align=\"center\" justify=\"center\">\n                                      <v-img\n                                        src=\"c2.png\"\n                                        max-height=\"110\"\n                                        max-width=\"110\"\n                                        contain\n                                      ></v-img>\n                                    </div>\n                                    <br />\n                                    <v-list-item-subtitle class=\"mt-2\">\n                                      <strong>{{ table.tableName }}</strong>\n                                    </v-list-item-subtitle>\n                                  </v-list-item-content>\n                                </v-list-item>\n                              </v-col>\n                            </v-row>\n                          </v-card>\n                        </v-item>\n                      </v-col>\n\n                      <!-- ----------------------------If Order Table ---------------------------- -->\n                      <v-col v-else>\n                        <router-link to=\"/menu\" class=\"tx-de\">\n                          <v-item>\n                            <v-card\n                              class=\"d-flex align-center rounded-lg mx-2\"\n                              height=\"140\"\n                              flat\n                            >\n                              <v-row>\n                                <v-col cols=\"12\" sm=\"12\">\n                                  <v-list-item\n                                    three-line\n                                    class=\"text-center\"\n                                    @click=\"chooseTable(table)\"\n                                  >\n                                    <v-list-item-content>\n                                      <div align=\"center\" justify=\"center\">\n                                        <v-img\n                                          src=\"c2.png\"\n                                          max-height=\"110\"\n                                          max-width=\"110\"\n                                          contain\n                                        ></v-img>\n                                      </div>\n                                      <br />\n                                      <v-list-item-subtitle class=\"mt-2\">\n                                        <strong>{{ table.tableName }}</strong>\n                                      </v-list-item-subtitle>\n                                    </v-list-item-content>\n                                  </v-list-item>\n                                </v-col>\n                              </v-row>\n                            </v-card>\n                          </v-item>\n                        </router-link>\n                      </v-col>\n                    </v-card>\n                    <!-- ---------------------------- End Table ---------------------------- -->\n                  </v-row>\n                </v-container>\n              </v-item-group>\n            </v-container>\n          </template>\n          <v-card>\n            <v-card-title class=\"text-h5 grey lighten-2\">\n              Payment\n            </v-card-title>\n\n            <v-card-text>\n              <bill-payment v-on:payment_dialog=\"payment_dialog\"></bill-payment>\n            </v-card-text>\n          </v-card>\n        </v-dialog>\n      </div>\n    </template>\n  </v-app>\n</template>\n\n<script>\nimport SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\n\nimport BillPayment from \"@/components/BillPayment.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"TableView\",\n  components: {\n    SideBar,\n    SideBarRight,\n    BillPayment,\n  },\n  data() {\n    return {\n      payment_dialog: false,\n      tableList: [],\n      table: {},\n    };\n  },\n  async created() {\n    await this.fetchAllTables();\n  },\n  methods: {\n    chooseTable(table) {\n      this.$store.dispatch(\"chooseTable\", {\n        table: table,\n      });\n    },\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list_with_order\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    },\n  },\n};\n</script>\n<style>\n.tx-de {\n  text-decoration: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}