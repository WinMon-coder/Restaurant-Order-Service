{"ast":null,"code":"import SideBar from \"@/components/SideBar.vue\";\nimport api from \"@/util/api\";\nexport default {\n  name: \"AdminCategoryView\",\n  components: {\n    SideBar\n  },\n  data: () => ({\n    dialog: false,\n    dialogDelete: false,\n    headers: [{\n      text: \"Dessert\",\n      align: \"start\",\n      sortable: false,\n      value: \"categoryName\"\n    }, {\n      text: \"Actions\",\n      value: \"actions\",\n      sortable: false\n    }],\n    categoryList: [],\n    editedIndex: -1,\n    editedItem: {\n      categoryName: \"\"\n    },\n    defaultItem: {\n      categoryName: \"\"\n    }\n  }),\n  computed: {\n    formTitle() {\n      return this.editedIndex === -1 ? \"New Item\" : \"Edit Item\";\n    }\n  },\n  watch: {\n    dialog(val) {\n      val || this.close();\n    },\n    dialogDelete(val) {\n      val || this.closeDelete();\n    }\n  },\n  async created() {\n    this.initialize();\n    await this.fetchAllCategories();\n  },\n  methods: {\n    initialize() {\n      this.categoryList = [];\n    },\n    async fetchAllCategories() {\n      const response = await api.get(\"/category/list\");\n      if (response.status === 200) {\n        const data = await response.json();\n        this.categoryList = data;\n      }\n    },\n    editItem(item) {\n      this.editedIndex = this.categoryList.indexOf(item);\n      this.editedItem = Object.assign({}, item);\n      this.dialog = true;\n    },\n    deleteItem(item) {\n      this.editedIndex = this.categoryList.indexOf(item);\n      this.editedItem = Object.assign({}, item);\n      this.dialogDelete = true;\n    },\n    deleteItemConfirm() {\n      this.categoryList.splice(this.editedIndex, 1);\n      this.closeDelete();\n    },\n    close() {\n      this.dialog = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    closeDelete() {\n      this.dialogDelete = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n    async save() {\n      if (this.editedIndex > -1) {\n        Object.assign(this.categoryList[this.editedIndex], this.editedItem);\n      } else {\n        const resp = await api.save(\"/category/create\", {\n          categoryName: this.newItem.categoryName\n        });\n        if (resp) {\n          await this.fetchAllCategories();\n          this.dialog = false;\n          this.newItem = {};\n        }\n      }\n      this.close();\n    }\n  }\n};","map":{"version":3,"mappings":"AAuFA;AACA;AACA;EACAA;EACAC;IACAC;EACA;EAEAC;IACAC;IACAC;IACAC,UACA;MACAC;MACAC;MACAC;MACAC;IACA,GACA;MAAAH;MAAAG;MAAAD;IAAA,EACA;IACAE;IACAC;IACAC;MACAC;IACA;IACAC;MACAD;IACA;EACA;EAEAE;IACAC;MACA;IACA;EACA;EAEAC;IACAd;MACAe;IACA;IACAd;MACAc;IACA;EACA;EAEA;IACA;IACA;EACA;EAEAC;IACAC;MACA;IACA;IACA;MACA;MACA;QACA;QACA;MACA;IACA;IAEAC;MACA;MACA;MACA;IACA;IAEAC;MACA;MACA;MACA;IACA;IAEAC;MACA;MACA;IACA;IAEAC;MACA;MACA;QACA;QACA;MACA;IACA;IAEAC;MACA;MACA;QACA;QACA;MACA;IACA;IAEA;MACA;QACAC;MACA;QACA;UACAb;QACA;QACA;UACA;UACA;UACA;QACA;MACA;MACA;IACA;EACA;AACA","names":["name","components","SideBar","data","dialog","dialogDelete","headers","text","align","sortable","value","categoryList","editedIndex","editedItem","categoryName","defaultItem","computed","formTitle","watch","val","methods","initialize","editItem","deleteItem","deleteItemConfirm","close","closeDelete","Object"],"sourceRoot":"src/views","sources":["AdminCategoryView.vue"],"sourcesContent":["<template>\n  <div>\n    <SideBar />\n\n    <template>\n      <v-data-table\n        :headers=\"headers\"\n        :items=\"categoryList\"\n        class=\"elevation-1\"\n      >\n        <template v-slot:top>\n          <v-toolbar flat>\n            <v-toolbar-title>My CRUD</v-toolbar-title>\n            <v-divider class=\"mx-4\" inset vertical></v-divider>\n            <v-spacer></v-spacer>\n            <v-dialog v-model