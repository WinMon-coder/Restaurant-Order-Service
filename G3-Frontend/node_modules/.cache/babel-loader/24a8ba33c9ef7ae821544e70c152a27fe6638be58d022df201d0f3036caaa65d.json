cer></v-spacer>\n                </v-card-actions>\n              </v-card>\n            </v-dialog>\n          </v-toolbar>\n        </template>\n        <template v-slot:[`item.actions`]=\"{ item }\">\n          <v-icon small class=\"mr-2\" @click=\"editItem(item)\">\n            mdi-pencil\n          </v-icon>\n          <v-icon small @click=\"deleteItem(item)\"> mdi-delete </v-icon>\n        </template>\n        <template v-slot:no-data>\n          <v-btn color=\"primary\" @click=\"initialize\"> Reset </v-btn>\n        </template>\n      </v-data-table>\n    </template>\n  </div>\n</template>\n<script>\nimport SideBar from \"@/components/SideBar.vue\";\nimport api from \"@/util/api\";\nexport default {\n  name: \"AdminCategoryView\",\n  components: {\n    SideBar,\n  },\n\n  data: () => ({\n    dialog: false,\n    dialogDelete: false,\n    headers: [\n      {\n        text: \"Dessert\",\n        align: \"start\",\n        sortable: false,\n        value: \"categoryName\",\n      },\n      { text: \"Actions\", value: \"actions\", sortable: false },\n    ],\n    categoryList: [],\n    editedIndex: -1,\n    editedItem: {\n      categoryName: \"\",\n    },\n    defaultItem: {\n      categoryName: \"\",\n    },\n  }),\n\n  computed: {\n    formTitle() {\n      return this.editedIndex === -1 ? \"New Item\" : \"Edit Item\";\n    },\n  },\n\n  watch: {\n    dialog(val) {\n      val || this.close();\n    },\n    dialogDelete(val) {\n      val || this.closeDelete();\n    },\n  },\n\n  async created() {\n    this.initialize();\n    await this.fetchAllCategories();\n  },\n\n  methods: {\n    initialize() {\n      this.categoryList = [];\n    },\n    async fetchAllCategories() {\n      const response = await api.get(\"/category/list\");\n      if (response.status === 200) {\n        const data = await response.json();\n        this.categoryList = data;\n      }\n    },\n\n    editItem(item) {\n      this.editedIndex = this.categoryList.indexOf(item);\n      this.editedItem = Object.assign({}, item);\n      this.dialog = true;\n    },\n\n    deleteItem(item) {\n      this.editedIndex = this.categoryList.indexOf(item);\n      this.editedItem = Object.assign({}, item);\n      this.dialogDelete = true;\n    },\n\n    deleteItemConfirm() {\n      this.categoryList.splice(this.editedIndex, 1);\n      this.closeDelete();\n    },\n\n    close() {\n      this.dialog = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n\n    closeDelete() {\n      this.dialogDelete = false;\n      this.$nextTick(() => {\n        this.editedItem = Object.assign({}, this.defaultItem);\n        this.editedIndex = -1;\n      });\n    },\n\n    async save() {\n      if (this.editedIndex > -1) {\n        Object.assign(this.categoryList[this.editedIndex], this.editedItem);\n      } else {\n        const resp = await api.save(\"/category/create\", {\n          categoryName: this.newItem.categoryName,\n        });\n        if (resp) {\n          await this.fetchAllCategories();\n          this.dialog = false;\n          this.newItem = {};\n        }\n      }\n      this.close();\n    },\n  },\n};\n</script>\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}