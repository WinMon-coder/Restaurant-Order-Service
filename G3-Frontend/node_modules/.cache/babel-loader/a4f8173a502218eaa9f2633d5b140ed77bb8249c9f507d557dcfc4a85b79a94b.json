em v-slot=\"{ active, toggle }\">\n                      <v-card\n                        color=\"#F6EFEF\"\n                        :class=\"active ? 'borderme' : ''\"\n                        class=\"d-flex align-center rounded-lg\"\n                        dark\n                        height=\"70\"\n                        @click=\"toggle\"\n                        flat\n                      >\n                        <v-row>\n                          <v-col cols=\"12\" md=\"12\">\n                            <v-list-item three-line class=\"text-center mt-1\">\n                              <v-list-item-content>\n                                <div>\n                                  <v-icon :color=\"active ? '#704232' : 'black'\"\n                                    >fas fa-money-bill-wave</v-icon\n                                  >\n                                </div>\n                                <v-list-item-subtitle\n                                  :class=\"\n                                    active ? 'brown--text' : 'black--text'\n                                  \"\n                                  class=\"mt-n2 caption\"\n                                  >Cach</v-list-item-subtitle\n                                >\n                              </v-list-item-content>\n                            </v-list-item>\n                          </v-col>\n                        </v-row>\n                      </v-card>\n                    </v-item>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"4\">\n                    <!-- ---------------------------- Payment For Credit  ---------------------------- -->\n                    <v-item v-slot=\"{ active, toggle }\">\n                      <v-card\n                        color=\"#F6EFEF\"\n                        :class=\"active ? 'borderme' : ''\"\n                        class=\"d-flex align-center rounded-lg\"\n                        dark\n                        height=\"70\"\n                        @click=\"toggle\"\n                        flat\n                      >\n                        <v-row>\n                          <v-col cols=\"12\" md=\"12\">\n                            <v-list-item three-line class=\"text-center mt-1\">\n                              <v-list-item-content>\n                                <div>\n                                  <v-icon :color=\"active ? '#704232' : 'black'\"\n                                    >fas fa-credit-card</v-icon\n                                  >\n                                </div>\n                                <v-list-item-subtitle\n                                  :class=\"\n                                    active ? 'brown--text' : 'black--text'\n                                  \"\n                                  class=\"mt-n2 caption\"\n                                  >debit Card</v-list-item-subtitle\n                                >\n                              </v-list-item-content>\n                            </v-list-item>\n                          </v-col>\n                        </v-row>\n                      </v-card>\n                    </v-item>\n                  </v-col>\n                  <v-col cols=\"12\" md=\"4\">\n                    <!-- ---------------------------- Payment For E-Wallet  ---------------------------- -->\n                    <v-item v-slot=\"{ active, toggle }\">\n                      <v-card\n                        color=\"#F6EFEF\"\n                        :class=\"active ? 'borderme' : ''\"\n                        class=\"d-flex align-center rounded-lg\"\n                        dark\n                        height=\"70\"\n                        @click=\"toggle\"\n                        flat\n                      >\n                        <v-row>\n                          <v-col cols=\"12\" md=\"12\">\n                            <v-list-item three-line class=\"text-center mt-1\">\n                              <v-list-item-content>\n                                <div>\n                                  <v-icon :color=\"active ? '#704232' : 'black'\"\n                                    >fas fa-qrcode</v-icon\n                                  >\n                                </div>\n                                <v-list-item-subtitle\n                                  :class=\"\n                                    active ? 'brown--text' : 'black--text'\n                                  \"\n                                  class=\"mt-n2 caption\"\n                                  >E-Wallet</v-list-item-subtitle\n                                >\n                              </v-list-item-content>\n                            </v-list-item>\n                          </v-col>\n                        </v-row>\n                      </v-card>\n                    </v-item>\n                  </v-col>\n                </v-row>\n                <v-row class=\"mt-2\">\n                  <v-col>\n                    <v-btn\n                      @click=\"updateOrderStatus(getTable.table.order.orderId)\"\n                      color=\"#F6EFEF\"\n                      block\n                      dark\n                      class=\"widthoutupercase\"\n                      >Payment\n                    </v-btn>\n                  </v-col>\n                  <v-col>\n                    <v-btn\n                      @click=\"closePayment()\"\n                      color=\"#F6EFEF\"\n                      block\n                      dark\n                      class=\"widthoutupercase\"\n                      >Cancel</v-btn\n                    >\n                  </v-col>\n                </v-row>\n              </v-container>\n            </v-item-group>\n          </v-container>\n        </template>\n      </v-col>\n    </v-row>\n  </div>\n</template>\n\n<script>\nimport SideBar from \"@/components/SideBar.vue\";\n\nexport default {\n  name: \"O