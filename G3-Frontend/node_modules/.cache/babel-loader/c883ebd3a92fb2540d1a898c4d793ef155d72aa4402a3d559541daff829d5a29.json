{"ast":null,"code":"import { mapGetters } from \"vuex\";\nimport utils from \"@/utils/utils\";\nexport default {\n  data: () => ({\n    onPay: 0,\n    localDomain: utils.constant.localDomain,\n    updateDemo: {\n      orderId: \"\"\n    }\n  }),\n  computed: {\n    ...mapGetters([\"getTable\"])\n  },\n  methods: {\n    // <!-- -------------****** update user from utils.http ******-------------------------- -->\n    async updateOrderStatus(id) {\n      this.updateDemo.orderId = id;\n      const resp = await utils.http.put(\"/user/update/order_status/\", {\n        orderId: this.updateDemo.orderId\n      });\n      if (resp && resp.status === 200) {\n        // Refresh User List\n        console.log(resp);\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAiMA;AACA;AACA;EACAA;IACAC;IACAC;IACAC;MAAAC;IAAA;EACA;EAEAC;IACA;EACA;EACAC;IACA;IACA;MACA;MACA;QACAF;MACA;MAEA;QACA;QACAG;MACA;IACA;EACA;AACA","names":["data","onPay","localDomain","updateDemo","orderId","computed","methods","console"],"sourceRoot":"src/components","sources":["BillPayment.vue"],"sourcesContent":["<template>\n  <v-container>\n    <strong class=\"ml-3\"\n      >Bills For {{ getTable.table.tableName }} Order Id :\n      {{ getTable.table.order.orderId }}</strong\n    >\n    <!-- ---------------------------- Order Items ---------------------------- -->\n    <v-list\n      subheader\n      two-line\n      class=\"mt-1\"\n      v-for=\"item in getTable.table.order.orderItems\"\n      :key=\"item.orderItemId\"\n    >\n      <v-list-item>\n        <v-list-item-avatar rounded color=\"grey lighten-4\">\n          <v-img :src=\"localDomain + item.food.foodPhoto\"></v-img>\n        </v-list-item-avatar>\n        <v-list-item-content>\n          <v-list-item-title class=\"subtitle-2\">{{\n            item.food.foodName\n          }}</v-list-item-title>\n          <v-list-item-subtitle>\n            {{ item.quantity }} X\n            <strong>{{ item.food.foodPrice }} Ks</strong>\n          </v-list-item-subtitle>\n        </v-list-item-content>\n        <v-list-item-action class=\"caption\"\n          >{{ item.quantity * item.food.foodPrice }}Ks\n        </v-list-item-action>\n      </v-list-item>\n    </v-list>\n\n    <!-- ---------------------------- End Order Items  ---------------------------- -->\n    <!-- <v-toolbar flat>\n      <strong>Subtotal</strong><v-spacer></v-spacer><strong>Ks</strong>\n    </v-toolbar>\n    <v-toolbar flat class=\"mt-n6\">\n      <span>Tax(10%)</span><v-spacer></v-spacer><span>350Ks</span>\n    </v-toolbar> -->\n    <v-divider class=\"mx-4\"></v-divider>\n    <v-toolbar flat>\n      <strong>Total:</strong><v-spacer></v-spacer\n      ><strong>{{ getTable.table.billTotal }}Ks</strong>\n    </v-toolbar>\n    <v-toolbar flat>\n      <strong>Net Pay:</strong><v-spacer></v-spacer><v-spacer></v-spacer>\n      <strong> {{ onPay }}Ks</strong>\n    </v-toolbar>\n\n    <v-toolbar flat>\n      <strong>Change : </strong><v-spacer></v-spacer>\n\n      <strong v-if=\"onPay <= 0\"> 0Ks </strong>\n      <strong> {{ getTable.table.billTotal - onPay }}Ks </strong>\n    </v-toolbar>\n    <v-text-field\n      type=\"number\"\n      v-model=\"onPay\"\n      solo\n      label=\"Solo\"\n      clearable\n    ></v-text-field>\n    <!-- ---------------------------- End Total  ---------------------------- -->\n\n    <!-- ---------------------------- Payment Method ---------------------------- -->\n    <strong class=\"ml-5\">Payment Method</strong>\n    <v-item-group mandatory class=\"mt-n1\">\n      <v-container>\n        <v-row justify=\"center\">\n          <!-- ---------------------------- Payment For Cash  ---------------------------- -->\n          <v-col cols=\"12\" md=\"4\">\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#704232' : 'black'\"\n                            >fas fa-money-bill-wave</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >Cach</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For Credit  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#704232' : 'black'\"\n                            >fas fa-credit-card</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >debit Card</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For E-Wallet  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#704232' : 'black'\"\n                            >fas fa-qrcode</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >E-Wallet</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n        </v-row>\n        <v-row class=\"mt-2\">\n          <v-col>\n            <v-btn\n              @click=\"updateOrderStatus(getTable.table.order.orderId)\"\n              color=\"#F6EFEF\"\n              block\n              dark\n              class=\"widthoutupercase\"\n              >Payment\n            </v-btn>\n          </v-col>\n          <v-col>\n            <v-btn color=\"#F6EFEF\" block dark class=\"widthoutupercase\"\n              >Cancel</v-btn\n            >\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-item-group>\n  </v-container>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport utils from \"@/utils/utils\";\nexport default {\n  data: () => ({\n    onPay: 0,\n    localDomain: utils.constant.localDomain,\n    updateDemo: { orderId: \"\" },\n  }),\n\n  computed: {\n    ...mapGetters([\"getTable\"]),\n  },\n  methods: {\n    // <!-- -------------****** update user from utils.http ******-------------------------- -->\n    async updateOrderStatus(id) {\n      this.updateDemo.orderId = id;\n      const resp = await utils.http.put(\"/user/update/order_status/\", {\n        orderId: this.updateDemo.orderId,\n      });\n\n      if (resp && resp.status === 200) {\n        // Refresh User List\n        console.log(resp);\n      }\n    },\n  },\n};\n</script>\n\n<style></style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}