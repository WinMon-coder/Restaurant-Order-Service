{"ast":null,"code":"import SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\nimport BillPayment from \"@/components/BillPayment.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"TableView\",\n  components: {\n    SideBar,\n    SideBarRight,\n    BillPayment\n  },\n  data: () => ({\n    tableList: [],\n    table: {}\n  }),\n  mounted() {\n    this.$store.dispatch(\"chooseTable\");\n    //this.$store.dispatch(\"getFood\", this.foodId);\n  },\n\n  async created() {\n    await this.fetchAllTables();\n  },\n  methods: {\n    clickOnToPayment(table) {\n      this.table = table;\n    },\n    chooseTable() {\n      this.$store.dispatch(\"chooseTable\", {\n        table: this.table\n      });\n    },\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list_with_order\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAkGA;AACA;AAEA;AACA;AACA;EACAA;EACAC;IACAC;IACAC;IACAC;EACA;EACAC;IACAC;IACAC;EACA;EACAC;IACA;IACA;EACA;;EACA;IACA;EACA;EAEAC;IACAC;MACA;IACA;IACAC;MACA;QACAJ;MACA;IACA;IAEA;IACA;MACA;MACA;QACA;QACA;MACA;QACAK;MACA;IACA;EACA;AACA","names":["name","components","SideBar","SideBarRight","BillPayment","data","tableList","table","mounted","methods","clickOnToPayment","chooseTable","console"],"sourceRoot":"src/views","sources":["TableView.vue"],"sourcesContent":["<template>\n  <v-app :style=\"{ background: $vuetify.theme.themes.dark.background }\">\n    <SideBar />\n    <SideBarRight />\n    <v-dialog v-model=\"dialog\" width=\"500\">\n      <template v-slot:activator=\"{ on, attrs }\">\n        <v-container class=\"mt-4\">\n          <v-row justify=\"center\" class=\"space\" v-bind=\"attrs\" v-on=\"on\">\n            <!-- ---------------------------- For Payment Table ---------------------------- -->\n            <v-col\n              cols=\"12\"\n              md=\"2\"\n              v-for=\"table in tableList\"\n              :key=\"table.tableId\"\n            >\n              <v-item>\n                <v-card\n                  class=\"d-flex align-center rounded-lg mx-2\"\n                  height=\"140\"\n                  flat\n                >\n                  <v-row>\n                    <v-col cols=\"12\" sm=\"12\">\n                      <v-list-item\n                        three-line\n                        class=\"text-center\"\n                        v-if=\"table.billTotal > 0\"\n                        :v-bind=\"attrs\"\n                        :v-on=\"on\"\n                        @click=\"clickOnToPayment(table)\"\n                      >\n                        <v-list-item-content>\n                          <div align=\"center\" justify=\"center\">\n                            <v-img\n                              src=\"c2.png\"\n                              max-height=\"110\"\n                              max-width=\"110\"\n                              contain\n                            ></v-img>\n                          </div>\n                          <v-list-item-subtitle class=\"mt-4\">\n                            <strong>{{ table.tableName }}</strong>\n                            <br />\n                            <strong\n                              >Total - {{ table.billTotal }}</strong\n                            ></v-list-item-subtitle\n                          >\n                        </v-list-item-content>\n                      </v-list-item>\n                      <v-list-item\n                        three-line\n                        class=\"text-center\"\n                        v-else\n                        to=\"/menu\"\n                        @click=\"chooseTable()\"\n                      >\n                        <v-list-item-content>\n                          <div align=\"center\" justify=\"center\">\n                            <v-img\n                              src=\"c2.png\"\n                              max-height=\"110\"\n                              max-width=\"110\"\n                              contain\n                            ></v-img>\n                          </div>\n                          <v-list-item-subtitle class=\"mt-4\">\n                            <strong>{{ table.tableName }}</strong>\n                            <br />\n                            <strong\n                              >Total - {{ table.billTotal }}</strong\n                            ></v-list-item-subtitle\n                          >\n                        </v-list-item-content>\n                      </v-list-item>\n                    </v-col>\n                  </v-row>\n                </v-card>\n              </v-item>\n            </v-col>\n\n            <!-- ----------------------------End For Payment Table  ---------------------------- -->\n          </v-row>\n        </v-container>\n      </template>\n      <v-card>\n        <v-card-title class=\"text-h5 grey lighten-2\">\n          Payment For : {{ table.tableName }}</v-card-title\n        >\n\n        <v-card-text>\n          <bill-payment :table=\"table\"></bill-payment>\n        </v-card-text>\n      </v-card>\n    </v-dialog>\n  </v-app>\n</template>\n\n<script>\nimport SideBar from \"../components/SideBar\";\nimport SideBarRight from \"../components/SideBarRight\";\n\nimport BillPayment from \"@/components/BillPayment.vue\";\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"TableView\",\n  components: {\n    SideBar,\n    SideBarRight,\n    BillPayment,\n  },\n  data: () => ({\n    tableList: [],\n    table: {},\n  }),\n  mounted() {\n    this.$store.dispatch(\"chooseTable\");\n    //this.$store.dispatch(\"getFood\", this.foodId);\n  },\n  async created() {\n    await this.fetchAllTables();\n  },\n\n  methods: {\n    clickOnToPayment(table) {\n      this.table = table;\n    },\n    chooseTable() {\n      this.$store.dispatch(\"chooseTable\", {\n        table: this.table,\n      });\n    },\n\n    // <!-- -------------****** get all table from utils.http ******-------------------------- -->\n    async fetchAllTables() {\n      const response = await utils.http.get(\"/table/list_with_order\");\n      if (response) {\n        const data = await response.json();\n        if (data) this.tableList = data;\n      } else {\n        console.log(\"sth wromg\");\n      }\n    },\n  },\n};\n</script>\n<style>\n.tx-de {\n  text-decoration: none;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}