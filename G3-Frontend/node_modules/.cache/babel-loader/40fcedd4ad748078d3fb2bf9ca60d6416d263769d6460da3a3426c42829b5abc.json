{"ast":null,"code":"import utils from \"@/utils/utils\";\nexport default {\n  name: \"SideBarRight\",\n  data: () => ({\n    localDomain: utils.constant.localDomain,\n    loading: false,\n    errorAlert: false,\n    orderDetail: {\n      orderUser: \"\",\n      orderTable: \"\",\n      foodList: [],\n      orderTotal: 0\n    }\n  }),\n  mounted() {\n    this.$store.dispatch(\"chooseTable\");\n    //this.$store.dispatch(\"getFood\", this.foodId);\n  },\n\n  components: {},\n  computed: {\n    cart() {\n      return this.$store.state.cart;\n    },\n    table() {\n      return this.$store.state.table;\n    },\n    orderTotalPrice() {\n      return this.$store.getters.orderTotalPrice;\n    }\n  },\n  methods: {\n    saveOrder() {\n      console.log(this.cart);\n      console.log(this.table);\n      // const resp = await utils.http.post(\"/user/order/create\", {\n      //   orderUser: this.$store.getters.loginUser,\n      //   foodList: this.$store.getters.cart.food,\n      //   orderTable: this.$store.getters.table,\n      //   orderTotal: this.orderTotalPrice,\n      // });\n\n      // console.log(this.orderDetail.orderUser);\n      // console.log(this.orderDetail.orderTable);\n      // console.log(this.orderDetail.foodList);\n      // console.log(this.orderDetail.orderTotal);\n      // if (resp) {\n      //   console.log(resp);\n      // }\n    },\n\n    removeFoodFromOrder(food) {\n      this.$store.dispatch(\"removeFoodFromOrder\", food);\n    },\n    cancelOrderItems() {\n      this.$store.dispatch(\"cancelOrderItems\");\n    }\n  }\n};","map":{"version":3,"mappings":"AAiNA;AACA;EACAA;EACAC;IACAC;IACAC;IACAC;IAEAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;IACA;EACA;;EACAC;EAEAC;IACAC;MACA;IACA;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;EACAC;IACAC;MACAC;MACAA;MACA;MACA;MACA;MACA;MACA;MACA;;MAEA;MACA;MACA;MACA;MACA;MACA;MACA;IACA;;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","localDomain","loading","errorAlert","orderDetail","orderUser","orderTable","foodList","orderTotal","mounted","components","computed","cart","table","orderTotalPrice","methods","saveOrder","console","removeFoodFromOrder","cancelOrderItems"],"sourceRoot":"src/components","sources":["SideBarRight.vue"],"sourcesContent":["<template>\n  <!-- ---------------------------- Bill Payment SideBar  ---------------------------- -->\n  <v-navigation-drawer app color=\"white\" right width=\"350\">\n    <v-list subheader two-line class=\"mt-1\">\n      <v-list-item>\n        <v-list-item-avatar rounded>\n          <v-img src=\"https://cdn.vuetifyjs.com/images/lists/2.jpg\"></v-img>\n        </v-list-item-avatar>\n        <v-list-item-content>\n          <v-list-item-subtitle>I'm a Waiter</v-list-item-subtitle>\n          <v-list-item-title>Mg Mg</v-list-item-title>\n        </v-list-item-content>\n      </v-list-item>\n    </v-list>\n    <strong class=\"ml-3\">Bills</strong>\n    <!-- ---------------------------- Order Items ---------------------------- -->\n    <div v-for=\"(item, index) in cart\" :key=\"index\">\n      <v-list subheader two-line>\n        <v-list-item>\n          <v-list-item-avatar rounded color=\"grey lighten-4\">\n            <v-img :src=\"localDomain + item.food.foodPhoto\"></v-img>\n          </v-list-item-avatar>\n          <v-row>\n            <v-col cols=\"12\" md=\"8\">\n              <v-list-item-content>\n                <v-list-item-title class=\"subtitle-2\">{{\n                  item.food.foodName\n                }}</v-list-item-title>\n                <v-list-item-subtitle\n                  >{{ item.quantity }} X\n                  <strong>{{ item.food.foodPrice }}Ks</strong>\n                  <!-- <v-btn plain color=\"#800015\" small\n                >No Ice\n                <v-icon right>mdi-pencil</v-icon>\n              </v-btn> -->\n                </v-list-item-subtitle>\n              </v-list-item-content>\n            </v-col>\n            <v-col cols=\"12\" md=\"4\">\n              <v-list-item>\n          <v-btn class=\"mx-2\" fab dark small color=\"primary\" @click=\"increase(item.food)\">              \n              <v-icon dark>mdi-plus</v-icon>\n          </v-btn>\n          <v-btn class=\"mx-2\" fab dark small color=\"primary\" @click=\"decrease(item.food)\">                    \n              <v-icon dark>mdi-minus</v-icon>\n          </v-btn>\n<v-list-item>\n            </v-col>\n          </v-row>\n          <v-list-item-action class=\"caption\"\n            >{{ item.quantity * item.food.foodPrice }}Ks\n            <v-icon\n              x-small\n              class=\"red--text\"\n              @click.prevent=\"removeFoodFromOrder(item.food)\"\n            >\n              fas fa-times\n            </v-icon>\n          </v-list-item-action>\n        </v-list-item>\n      </v-list>\n    </div>\n    <!-- ---------------------------- End Order Items  ---------------------------- -->\n    <!-- <v-toolbar flat>\n      <strong>Subtotal</strong><v-spacer></v-spacer><strong>3500Ks</strong>\n    </v-toolbar>\n    <v-toolbar flat class=\"mt-n6\">\n      <span>Tax(10%)</span><v-spacer></v-spacer><span>350Ks</span>\n    </v-toolbar> -->\n    <v-divider class=\"mx-4\"></v-divider>\n    <v-toolbar flat>\n      <strong>Total</strong><v-spacer></v-spacer\n      ><strong>{{ orderTotalPrice }}Ks</strong>\n    </v-toolbar>\n    <!-- ---------------------------- End Total  ---------------------------- -->\n\n    <!-- ---------------------------- Payment Method ---------------------------- -->\n    <strong class=\"ml-5\">Payment Method</strong>\n    <v-item-group mandatory class=\"mt-n1\">\n      <v-container>\n        <v-row justify=\"center\">\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For Cash  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-money-bill-wave</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >Cach</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For Credit  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-credit-card</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >debit Card</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n          <v-col cols=\"12\" md=\"4\">\n            <!-- ---------------------------- Payment For E-Wallet  ---------------------------- -->\n            <v-item v-slot=\"{ active, toggle }\">\n              <v-card\n                color=\"#F6EFEF\"\n                :class=\"active ? 'borderme' : ''\"\n                class=\"d-flex align-center rounded-lg\"\n                dark\n                height=\"70\"\n                @click=\"toggle\"\n                flat\n              >\n                <v-row>\n                  <v-col cols=\"12\" md=\"12\">\n                    <v-list-item three-line class=\"text-center mt-1\">\n                      <v-list-item-content>\n                        <div>\n                          <v-icon :color=\"active ? '#800015' : 'black'\"\n                            >fas fa-qrcode</v-icon\n                          >\n                        </div>\n                        <v-list-item-subtitle\n                          :class=\"active ? 'brown--text' : 'black--text'\"\n                          class=\"mt-n2 caption\"\n                          >E-Wallet</v-list-item-subtitle\n                        >\n                      </v-list-item-content>\n                    </v-list-item>\n                  </v-col>\n                </v-row>\n              </v-card>\n            </v-item>\n          </v-col>\n        </v-row>\n        <v-row class=\"mt-1\">\n          <v-col>\n            <v-btn\n              color=\"#F6EFEF\"\n              block\n              dark\n              class=\"widthoutupercase\"\n              @click.prevent=\"saveOrder()\"\n              >Order\n            </v-btn>\n          </v-col>\n          <v-col>\n            <v-btn\n              color=\"#F6EFEF\"\n              block\n              dark\n              class=\"widthoutupercase\"\n              @click.prevent=\"cancelOrderItems()\"\n              >Cancel</v-btn\n            >\n          </v-col>\n        </v-row>\n      </v-container>\n    </v-item-group>\n  </v-navigation-drawer>\n</template>\n\n<script>\nimport utils from \"@/utils/utils\";\nexport default {\n  name: \"SideBarRight\",\n  data: () => ({\n    localDomain: utils.constant.localDomain,\n    loading: false,\n    errorAlert: false,\n\n    orderDetail: {\n      orderUser: \"\",\n      orderTable: \"\",\n      foodList: [],\n      orderTotal: 0,\n    },\n  }),\n  mounted() {\n    this.$store.dispatch(\"chooseTable\");\n    //this.$store.dispatch(\"getFood\", this.foodId);\n  },\n  components: {},\n\n  computed: {\n    cart() {\n      return this.$store.state.cart;\n    },\n    table() {\n      return this.$store.state.table;\n    },\n    orderTotalPrice() {\n      return this.$store.getters.orderTotalPrice;\n    },\n  },\n  methods: {\n    saveOrder() {\n      console.log(this.cart);\n      console.log(this.table);\n      // const resp = await utils.http.post(\"/user/order/create\", {\n      //   orderUser: this.$store.getters.loginUser,\n      //   foodList: this.$store.getters.cart.food,\n      //   orderTable: this.$store.getters.table,\n      //   orderTotal: this.orderTotalPrice,\n      // });\n\n      // console.log(this.orderDetail.orderUser);\n      // console.log(this.orderDetail.orderTable);\n      // console.log(this.orderDetail.foodList);\n      // console.log(this.orderDetail.orderTotal);\n      // if (resp) {\n      //   console.log(resp);\n      // }\n    },\n    removeFoodFromOrder(food) {\n      this.$store.dispatch(\"removeFoodFromOrder\", food);\n    },\n    cancelOrderItems() {\n      this.$store.dispatch(\"cancelOrderItems\");\n    },\n  },\n};\n</script>\n\n<style>\n.v-card.borderme {\n  border: 2px solid #800015 !important;\n}\n.col-12 {\n  padding: 5px !important;\n}\n.v-btn.widthoutupercase {\n  color: #800015 !important;\n  text-transform: none !important;\n}\n.v-btn:hover {\n  background-color: #800015 !important;\n  color: white !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}